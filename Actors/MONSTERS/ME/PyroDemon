    FSHT A 1 Bright A_SpawnItemEx("FireTail",0,0,0,0,0,0,0,128)
    FSHT A 1 Bright A_SpawnItemEx("FireTail",0,0,0,0,0,0,0,128)
    FSHT A 1 Bright A_SpawnItemEx("FireTail",0,0,0,0,0,0,0,128)
    FSHT A 1 Bright A_SpawnItemEx("FireTail",0,0,0,0,0,0,0,128)
    FSHT A 1 Bright A_SpawnItemEx("FireTail",0,0,0,0,0,0,0,128)
    FSHT A 1 Bright A_SpawnItemEx("FireTail",0,0,0,0,0,0,0,128)
    Loop
  Death:
    FSHT A 0 Bright 
    FSHT B 0 Bright
    FSHT B 3 Bright
    FSHT C 3 Bright
    FSHT D 3 Bright
    FSHT E 3 Bright
    Stop
  }
}

