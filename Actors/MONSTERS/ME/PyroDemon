Actor PyroDemon : BaronofHell 30105
{
  //$Title "Pyro Demon"
  //$Category XV117Monsters
  scale 1.2
  Health 1500
  Radius 24
  Height 64
  Mass 1000
  Speed 8
  painchance "acid", 255
  Painchance "shock", 256
  PainChance 20
  Painchance "beartrap", 255
  Monster
  MeleeDamage 10
  Obituary "%o was burnt to a crisp by an Infernal Prince"
  HitObituary "%o was melted by a pyro an Infernal Prince"
  Bloodcolor "75 25 00"
  +NoTarget
  +FloorClip
  +MissileMore
  +Boss
  +DontHurtSpecies
 	PainChance "Fire", 0
	PainChance "Flames", 0
	PainChance "Burn", 0
	damagefactor "fire", 0
	damagefactor "Flames", 0
	damagefactor "Burn", 0
	DamageFactor "Crush", 10.0
	DamageFactor "Use", 0.0
	DamageFactor "Stealth", 0.0
  SeeSound "pyro/see"
  PainSound "pyro/pain"
  DeathSound "pyro/death"
  ActiveSound "pyro/idle"
  
  DropItem "powderspawner" 70
  DropItem "powderspawner" 70
  DropItem "powderspawner" 120
	DropItem "fireessence" 250
  Tag "Infernal Prince"
  States
  {
	 SpawnNO:
			TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
		TNT1 A 0 A_ChangeFlag("COUNTKILL", 0)
		TNT1 A 0 A_SpawnItemEx ("BaronofHell2",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		Stop
	
	ReplaceForBoss1:
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
		TNT1 A 0 A_ChangeFlag("COUNTKILL", 0)
		TNT1 A 0 A_SpawnItemEx ("BaronBoss1",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG  | SXF_TRANSFERPOINTERS ,0, tid)
		TNT1 A 10 
		Stop	
		
	ReplaceForBoss2:
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
		TNT1 A 0 A_ChangeFlag("COUNTKILL", 0)
		TNT1 A 0 A_SpawnItemEx ("BaronBoss2",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG  | SXF_TRANSFERPOINTERS ,0, tid)
		TNT1 A 10 
		Stop	
	
	 Spawn:
	TNT1 A 0 
	TNT1 A 0 ACS_NamedExecuteWithResult("RADARSIGNAL",0)
	TNT1 A 0 A_GiveInventory("TargetIsABaronOfHell")
	//
	TNT1 A 0 A_JumpIf(x == 304, "ReplaceForBoss1")
	TNT1 A 0 A_JumpIf(x == 528, "ReplaceForBoss2")
	//
	
	Look:
    PYRO AB 10 A_Look
    Loop
	
  See:
    PYRO AA 3 A_Chase
    PYRO A 0 A_SpawnItemEx("WalkFire",0,0,0,1,0,0,0,128)
    PYRO BB 3 A_Chase
    PYRO B 0 A_SpawnItemEx("WalkFire",0,0,0,1,0,0,0,128)
    PYRO CC 3 A_Chase
    PYRO C 0 A_SpawnItemEx("WalkFire",0,0,0,1,0,0,0,128)
    PYRO DD 3 A_Chase
    PYRO D 0 A_SpawnItemEx("WalkFire",0,0,0,1,0,0,0,128)
	TNT1 A 0 A_JumpIfintargetInventory("predatormode", 1, "Look")
    Loop
	
	Melee:
	    TNT1 A 0
		TNT1 A 0 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 63,0)
		PYRO E 1
        PYRO F 6
        TNT1 A 0 A_PlaySound("weapons/gswing")
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 63,0)
		TNT1 A 0 a_checkproximity("Parried","Axeparry",80.0,1,CPXF_NOZ)
		TNT1 A 0 A_CustomMissile("BaronAttack",10,0,0,0)
		PYRO G 6 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 63,0)
		Goto See
		
  Missile:
    PYRO B 0 A_Jump(32,29)
    PYRO B 0 A_Jump(32,20)
    PYRO EF 8 Bright A_FaceTarget
    PYRO G 0 Bright A_CustomMissile ("FireShot",28,0,0,0)
    PYRO G 0 Bright A_CustomMissile ("FireShot",28,0,-4,0)
    PYRO G 3 Bright A_CustomMissile ("FireShot",28,0,4,0)
    PYRO H 3 Bright
    PYRO H 0 A_Jump(32,8)
    PYRO IJ 8 Bright A_FaceTarget 
    PYRO K 0 Bright A_CustomMissile ("FireShot",28,0,0,0)
    PYRO K 0 Bright A_CustomMissile ("FireShot",28,0,-4,0)
    PYRO K 3 Bright A_CustomMissile ("FireShot",28,0,4,0)
    PYRO L 3 Bright
    PYRO L 0 A_Jump(32,1)
    Goto See
    PYRO MN 8 Bright A_FaceTarget
    PYRO O 4 Bright A_CustomMissile ("PyroBlast",28,0,0,0)
    PYRO P 4 Bright
    Goto See
    PYRO MN 8 Bright A_FaceTarget
    PYRO O 4 Bright A_CustomMissile ("RainCometNull",28,0,0,0)
    PYRO O 4 Bright A_CustomMissile ("RainCometNull",28,0,90,0)
    PYRO O 4 Bright A_CustomMissile ("RainCometNull",28,0,180,0)
    PYRO O 4 Bright A_CustomMissile ("RainCometNull",28,0,270,0)
    PYRO P 4 Bright
    Goto See
	
	TELEPORT:
	PYRO O 1 A_FaceTarget
    PYRO O 0 A_CustomMissile ("EscapeExplosion",28,10,0,0)
    PYRO O 0 A_Mushroom ("Pyroball",16)
    PYRO O 3
    PYRO P 3 Bright
    PYRO A 0 A_UnSetShootable
    PYRO A 1 A_PlaySound("pyro/tele")
    PYRO A 1 A_SetTranslucent(0.90)
    PYRO A 1 A_SetTranslucent(0.80)
    PYRO A 1 A_SetTranslucent(0.70)
    PYRO A 1 A_SetTranslucent(0.60)
    PYRO A 1 A_SetTranslucent(0.50)
    PYRO A 1 A_SetTranslucent(0.40)
    PYRO A 1 A_SetTranslucent(0.30)
    PYRO A 1 A_SetTranslucent(0.20)
    PYRO A 1 A_SetTranslucent(0.10)
    TNT1 AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA 0 A_Wander
    TNT1 A 0 A_Jump(128,25)
    TNT1 AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA 0 A_ExtChase(0,0,1,1)
    PYRO A 1 A_PlaySound("pyro/tele")
    PYRO A 1 A_SetTranslucent(0.10)
    PYRO A 1 A_SetTranslucent(0.20)
    PYRO A 1 A_SetTranslucent(0.30)
    PYRO A 1 A_SetTranslucent(0.40)
    PYRO A 1 A_SetTranslucent(0.50)
    PYRO A 1 A_SetTranslucent(0.60)
    PYRO A 1 A_SetTranslucent(0.70)
    PYRO A 1 A_SetTranslucent(0.80)
    PYRO A 1 A_SetTranslucent(0.90)
    PYRO A 1 A_SetTranslucent(1.0)
    PYRO A 0 A_CustomMissile ("EscapeExplosion",28,10,0,0)
    PYRO O 0 A_Mushroom ("Pyroball",16)
    PYRO A 0 A_SetShootable
    Goto See
	
PAIN.SYRINGE2:
	TNT1 A 0 A_CHANGEFLAG("FORCEINFIGHTING",1)
	TNT1 A 0 A_CHANGEFLAG("DOHARMSPECIES",1)
	TNT1 A 0 A_CHANGEFLAG("DONTHARMSPECIES",0)
	TNT1 A 0 A_CHANGEFLAG("DONTHARMCLASS",0)
	TNT1 A 0 A_CHANGEFLAG("NOINFIGHTSPECIES",0)
	TNT1 A 0 A_CHANGEFLAG("NOINFIGHTING",0)
	TNT1 A 0 A_CLEARTARGET
	TNT1 A 0 A_EXPLODE(1,270,XF_NOTMISSILE | XF_EXPLICITDAMAGETYPE,0,270,0,0,"bulletpuff","frenzytrigger")
	goto see
PAIN.FRENZYTRIGGER:
	TNT1 A 0 Healthing(1)
	TNT1 A 0 A_CHANGEFLAG("FORCEINFIGHTING",1)
	TNT1 A 0 A_CHANGEFLAG("DOHARMSPECIES",1)
	TNT1 A 0 A_CHANGEFLAG("DONTHARMSPECIES",0)
	TNT1 A 0 A_CHANGEFLAG("DONTHARMCLASS",0)
	TNT1 A 0 A_CHANGEFLAG("NOINFIGHTSPECIES",0)
	TNT1 A 0 A_CHANGEFLAG("NOINFIGHTING",0)
  goto See
	
	Parried:
		TNT1 A 0 A_JumpIfintargetInventory("isdoomguy", 1, "Parry2")
		TNT1 A 0 A_JumpIfintargetInventory("LARGESTAMINA", 1, 2)
		TNT1 A 0 A_JumpifintargetInventory("UsedStamina",1,"CounterParry")
		TNT1 A 0 A_JumpifintargetInventory("UsedStamina",1900,"CounterParry")
		goto Parry2
	CounterParry:
		TNT1 A 0 A_startsound("KICKWALL",800)
		TNT1 A 0 A_givetotarget("triptoken",1)
		TNT1 A 0 A_takefromtarget("axedurability",30)
		TNT1 A 0 A_givetotarget("usedstamina",1000)
		TNT1 A 0 A_CustomMissile("KnockAttack",10,0,0,0)
		goto See
	Parry2:
		TNT1 A 0 A_CHANGEFLAG("NOPAIN",1)
		TNT1 A 0 A_startsound("PARRIED",801)
		TNT1 A 0 A_givetotarget("usedstamina",100)
		TNT1 A 0 A_givetotarget("parrytoken",1)
		TNT1 A 0 A_takefromtarget("axedurability",12)
		TNT1 A 0 A_FaceTarget
		TNT1 A 0 A_SpawnItemex ("parryeffect", 6, 0,30,0,0,0,0, SXF_SETMASTER)
		CBOS H 10 A_Pain
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 58,0)
		goto pain.acid
	
	PAIN.SYRINGE1:
	TNT1 A 0 A_JUMPIFinventory("syringedonce",1,3)
	TNT1 A 0 A_JUMPIFHEALTHLOWER(750,2)
	TNT1 A 0 A_GIVEINVENTORY("syringedonce",1)
	goto pain.acid
	tnt1 a 0 givebody(1500)
	TNT1 A 0 A_CHANGEFLAG("FRIENDLY",1)
	pain.acid:
	PYRO Q 10
    PYRO Q 10 A_Pain
	PYRO Q 10
    PYRO Q 10 A_Pain
  Pain:
    PYRO Q 2
    PYRO Q 2 A_Pain
	TNT1 A 0 A_CHANGEFLAG("NOPAIN",0)
    Goto See

Pain.Beartrap:
		TNT1 A 0  a_changeflag("DONTHRUST",1)
		TNT1 A 0 a_changeflag("NOPAIN",1)
		TNT1 A 0 a_spawnitem("beartrapTRIGGERED",0,0)
		PYRO Q 30 a_scream
		TNT1 A 0 a_changeflag("NOPAIN",0)
		TNT1 A 0 a_changeflag("DONTHRUST",0)
		goto TELEPORT

	 Death.Beartrap:
		TNT1 A 0 A_checkproximity("Death","beartrapTriggered",40,1)
		TNT1 A 0 a_spawnitem("beartrap",0,0)
		goto death
	
Death.ChainCut:
Death.ChainSaw:
		TNT1 A 0 A_SpawnItemEx("HealthPlusFatalityBonus", 0, 0, 20, random (-1, 1), random (-1, 1), 8)
		TNT1 A 0 A_SpawnItemEx("ArmorGem", 0, 0, 20, random (-1, 1), random (-1, 1), 8)	
		TNT1 A 0 A_SpawnItemEx("HealthPlusFatalityBonus", 0, 0, 20, random (-1, 1), random (-1, 1), 8)
		TNT1 A 0 A_SpawnItemEx("ArmorGem", 0, 0, 20, random (-1, 1), random (-1, 1), 8)		
Death.Cut:
Death.Saw:
  Death:
    PYRO R 10 Bright A_Scream
    PYRO S 10 Bright A_CustomMissile ("PlasmaSmoke", 28, 0, random (0, 180), 2, random (0, 180))
	 TNT1 A 0 A_CustomMissile("Shrapnel",0,0,0,2,0) 
	 TNT1 A 0 A_CustomMissile("SmokeSpawner",0,0,0,2,0)
    PYRO T 10 Bright A_CustomMissile ("PlasmaSmoke", 28, 0, random (0, 180), 2, random (0, 180)) 
	TNT1 A 0 A_CustomMissile("Shrapnel",0,0,0,2,0) 
    PYRO U 10 Bright A_NoBlocking
    PYRO VW 6 Bright A_CustomMissile ("PlasmaSmoke", 28, 0, random (0, 180), 2, random (0, 180))
	TNT1 A 0 A_CustomMissile("Shrapnel",0,0,0,2,0) 
	 TNT1 A 0 A_CustomMissile("SmokeSpawner",0,0,0,2,0)
    PYRO X 6 Bright A_CustomMissile ("PlasmaSmoke", 28, 0, random (0, 180), 2, random (0, 180))
	TNT1 A 0 A_CustomMissile("Shrapnel",0,0,0,2,0) 
    PYRO Y 3 Bright A_CustomMissile ("PlasmaSmoke", 28, 0, random (0, 180), 2, random (0, 180))
	 TNT1 A 0 A_CustomMissile("SmokeSpawner",0,0,0,2,0)
    PYRO Z -1 Bright A_CustomMissile ("PlasmaSmoke", 28, 0, random (0, 180), 2, random (0, 180))
    Stop
  }
}

