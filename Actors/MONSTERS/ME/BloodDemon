ACTOR BloodDemon 30102
{ 
	//$Category Monsters
	Species "Demon"
    Health 800
    Speed 8
    FastSpeed 10
    Height 70
    Radius 30
	scale 1.4
    Mass 1800
    DamageFactor "Explosive", 1.0
	damagefactor "Acid", 3.0
    PainChance "Saw", 200
	PainChance "Acid", 255
	Painchance "beartrap", 255
	Painchance "Shock", 256
	painchance "bleeding", 256 painchance "bleedingarmor", 20
    PainChance 25
Painchance "Slide", 0
	PainChance "Cut", 200
	PainChance "Taunt", 255
	PainChance "ExtremePunches", 255 PainChance "SuperPunch", 255
	PainChance "Kick", 255 PainChance "HangingHook", 0
    damagefactor "Crush", 10.0    damagefactor "SSG", 0.5 damagefactor "Shotgun",0.5
	damagefactor "Avoid", 0.0	damagefactor "GibRemoving", 0.0	damagefactor "SSG", 0.3	damagefactor "TeleportRemover", 0.0
	DamageFactor "CauseSplash", 0.0 DamageFactor "CauseObjectsToSplashSlime", 0.0 DamageFactor "CauseObjectsToSplashNukage", 0.0
	DamageFactor "CauseObjectsToSplashBlood", 0.0 DamageFactor "CauseObjectsToSplashLava", 0.0
	DamageFactor "Use", 0.0
	damagefactor "slug", 1.2
	DamageFactor "Stealth", 0.0
	damagefactor "Chaincut", 0.3 damagefactor "chainsaw", 0.3
    Meleedamage 5 
    REACTIONTIME 8 
	
MONSTER
damagefactor "Blood", 0.0 damagefactor "BlueBlood", 0.0 damagefactor "GreenBlood", 0.0
    +FLOORCLIP
  dropitem "heartspawner" 127
    SeeSound "blooddemon/sight"
    PainSound "blooddemon/pain"
    DeathSound "blooddemon/death"
    ActiveSound "blooddemon/active"
    //MeleeSound "blooddemon/melee"
    HitObituary "%o Was cyber-bullied by a CyberBull" 

BloodType "Brutal_Blood", "SawBlood", "SawBlood"
	DamageType Eat
	MaxStepHeight 32
	MaxDropOffHeight 32
	MeleeRange 85
	Tag "Cyber Bull"
	DropItem "DemonStrengthRune" 3
	dropitem "metalSPAWNER" 100
	dropitem "metalSPAWNER" 100

 States 
    { 
	
	SpawnNospace:
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
		TNT1 A 0 A_ChangeFlag("COUNTKILL", 0)
		TNT1 A 0 A_SpawnItemEx ("BuffBullDemon",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERPOINTERS ,0, tid)
		Stop
	
	ReplaceVanilla:
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
		TNT1 A 0 A_ChangeFlag("COUNTKILL", 0)
		TNT1 A 0 A_SpawnItemEx ("BullDemon",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERPOINTERS ,0, tid)
		Stop
	
    Spawn: 
		TNT1 A 0
		TNT1 A 0 ACS_NamedExecuteWithResult("RADARSIGNAL",0)
		//
		TNT1 A 0 A_Checkceiling("SpawnnoSpace")
		TNT1 A 0 A_JUMPIF(GETCVAR("monsterspawns")==4, "ReplaceVanilla")
		//
				
	Look:
	Givearmor:
		TNT1 A 0 A_JumpIf(GetCVAR("EnemyArmorValue")==true,"Givearmor2")
		TNT1 A 0 A_GiveInventory("100ARMORMED")
		TNT1 A 0 A_GiveInventory("hasPLATEarmor")
		goto STAND
	Givearmor2:	
		TNT1 A 0 A_GiveInventory("100ARMORHIGH")
		TNT1 A 0 A_GiveInventory("hasPLATEarmor")
		Goto STAND
		Goto Stand
	
	Searchforplayer:
	TNT1 a 0 a_cleartarget
	Stand:
       SRG2 AB 10 A_Look
       Loop 
    See: 
       SRG2 A 0 A_PlaySound("blooddemon/walk")
	   TNT1 A 0 A_Jumpifinventory("destroyedarmor", 1,2)
	   TNT1 A 0 A_Jumpifinventory("BasicArmor", 1,1)
	   goto LostArmor
       SRG2 AABB 2 A_Chase
	   TNT1 A 0 A_Jumpifinventory("destroyedarmor", 1,2)
	   TNT1 A 0 A_Jumpifinventory("BasicArmor", 1,1)
	   goto LostArmor
       SRG2 C 0 A_PlaySound("blooddemon/walk")
       SRG2 CCDD 2 A_Chase
	   	TNT1 A 0 A_JumpIfintargetInventory("predatormode", 1, "SearchforPlayer")
       Loop 
    Melee: 
		TNT1 A 0 A_PlaySound("blooddemon/melee", 4)
	   SRG2 E 0 A_PlaySound("blooddemon/melee")
       SRG2 EF 8 A_FaceTarget
	   TNT1 A 0 a_checkproximity("Parried","Axeparry",80.0,1,CPXF_NOZ)
       SRG2 G 8 A_CustomMeleeAttack(60,"blooddemon/melee","","Extreme")
	   TNT1 A 0 A_CustomMissile("Imprupture",10,0,0,0)
       Goto See 
	 
PAIN.SYRINGE2:
	TNT1 A 0 A_CHANGEFLAG("FORCEINFIGHTING",1)
	TNT1 A 0 A_CHANGEFLAG("DOHARMSPECIES",1)
	TNT1 A 0 A_CHANGEFLAG("DONTHARMSPECIES",0)
	TNT1 A 0 A_CHANGEFLAG("DONTHARMCLASS",0)
	TNT1 A 0 A_CHANGEFLAG("NOINFIGHTSPECIES",0)
	TNT1 A 0 A_CHANGEFLAG("NOINFIGHTING",0)
	TNT1 A 0 A_CLEARTARGET
	TNT1 A 0 A_EXPLODE(1,270,XF_NOTMISSILE | XF_EXPLICITDAMAGETYPE,0,270,0,0,"bulletpuff","frenzytrigger")
	goto see
PAIN.FRENZYTRIGGER:
	TNT1 A 0 Healthing(1)
	TNT1 A 0 A_CHANGEFLAG("FORCEINFIGHTING",1)
	TNT1 A 0 A_CHANGEFLAG("DOHARMSPECIES",1)
	TNT1 A 0 A_CHANGEFLAG("DONTHARMSPECIES",0)
	TNT1 A 0 A_CHANGEFLAG("DONTHARMCLASS",0)
	TNT1 A 0 A_CHANGEFLAG("NOINFIGHTSPECIES",0)
	TNT1 A 0 A_CHANGEFLAG("NOINFIGHTING",0)
  goto See

pain.bleedingarmor:		
Pain.Bleeding:
	TNT1 A 0 A_CustomMissile ("Brutal_FlyingBlood", 55, 0, random (0, 360), 2, random (0, 160))
	TNT1 A 0 a_Jump(140, "See")
	goto Pain

	Parried:
		TNT1 A 0 A_JumpIfintargetInventory("isdoomguy", 1, "Parry2")
		TNT1 A 0 A_JumpIfintargetInventory("LARGESTAMINA", 1, 2)
		TNT1 A 0 A_JumpifintargetInventory("UsedStamina",50,"CounterParry")
		TNT1 A 0 A_JumpifintargetInventory("UsedStamina",1950,"CounterParry")
		goto Parry2
	CounterParry:
		TNT1 A 0 A_startsound("KICKWALL",800)
		TNT1 A 0 A_givetotarget("triptoken",1)
		TNT1 A 0 A_takefromtarget("axedurability",20)
		TNT1 A 0 A_givetotarget("usedstamina",800)
		TNT1 A 0 A_CustomMissile("KnockAttack",10,0,0,0)
		goto See
	Parry2:
		TNT1 A 0 A_CHANGEFLAG("NOPAIN",1)
		TNT1 A 0 A_startsound("PARRIED",801)
		TNT1 A 0 A_givetotarget("usedstamina",50)
		TNT1 A 0 A_givetotarget("parrytoken",1)
		TNT1 A 0 A_takefromtarget("axedurability",8)
		TNT1 A 0 A_FaceTarget
		TNT1 A 0 A_SpawnItemex ("parryeffect", 6, 0,30,0,0,0,0, SXF_SETMASTER)
		 SRG2 H 30
		goto pain.acid
	
	  Pain.Shock:
	  TNT1 A 0 A_spawnitem("plasmaexplosion",0,30)
		TNT1 A 0 A_giveinventory("shockcharge",50)
		TNT1 A 0 A_playsound("Sparks/Spawn", CHAN_7)
		goto pain
	 pain.acid:  
	 SRG2 H 10
       SRG2 H 8 A_Pain
	   SRG2 H 10
       SRG2 H 8 A_Pain
	   SRG2 H 10
       SRG2 H 8
    Pain: 
		TNT1 A 0 A_CHANGEFLAG("NOPAIN",0)
	   TNT1 A 0 A_Jumpifinventory("destroyedarmor", 1,2)
	   TNT1 A 0 A_Jumpifinventory("BasicArmor", 1,1)
	   goto LostArmor
       SRG2 H 2
       SRG2 H 2 A_Pain
	   TNT1 A 0 A_JumpIfInventory ("shockcharge", 1, "Shocked")
       Goto See
	
	Shocked:
	TNT1 A 0 A_takeinventory("shockcharge",1)
	TNT1 A 0 A_spawnitem("plasmashockbeeg",0,50)
	TNT1 A 0 A_DamageSelf(2,"plasma")
	goto Pain
	
	  LostArmor:
		TNT1 A 0 A_log("the CyberBull's armor has been destroyed!")
	   TNT1 A 0 A_giveinventory("exposedarmor",1)
	   TNT1 A 0 A_giveinventory("destroyedarmor",1)
	   SRG2 H 3
	   TNT1 A 0 A_CustomMissile ("TerminatorDemonShoulder", 24, 0, random (0, 360), 2, random (0, 160))
	   SRG2 H 3 A_Pain
	    TNT1 A 0 A_Spawnitemex("scrapmetal",0,0,0,random(-5,5),random(-5,5), random(-5,5), random(0, 360), 0, 127)
	   SRG2 H 3
	   TNT1 A 0 A_CustomMissile ("TerminatorDemonShoulder", 24, 0, random (0, 360), 2, random (0, 160))
       SRG2 H 3 A_Pain
	   goto see

pain.beartrap:
TNT1 A 0 a_spawnitem("beartraprip",0,0)	
goto see

//    Death: 
//       SRG2 I 8 
//       SRG2 I 0 A_FaceTarget
 //      SRG2 J 0 A_SpawnItemEx("BloodDemonArm", 10, 0, 32, 0, 8, //0, 0, 128)
  //     SRG2 J 8 A_Scream
    //   SRG2 K 4 
      // SRG2 L 4 A_NoBlocking
       //SRG2 M 4 
       //SRG2 N -1 
//TNT1 A 0 A_SpawnItem ("GrowingBloodPool")
  //     Stop 


	Death:
	    TNT1 A 0
		TNT1 A 0 A_Jump(96, "LeftDeath")
        TNT1 A 0 A_Jump(96, 13)
	
	Death.Minor:
		TNT1 AA 0
		SRG3 A 8
		SRG3 B 8 A_Scream
		SRG3 C 4
		SRG3 D 4 A_NoBlocking
		SRG3 E 4
        TNT1 A 0 A_SpawnItem ("DeadBDemon1", 1)
		TNT1 A 0 A_SpawnItem ("GrowingBloodPool")
		Stop
        TNT1 AAAAAAAAA 0
        TNT1 A 0 A_CustomMissile ("MuchBlood", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("BloodDemonArm", 35, 0, random (0, 360), 2, random (0, 160))
		SRG3 B 8 A_CustomMissile ("XDeath1", 40, 0, random (0, 360), 2, random (0, 160))
		SRG3 B 8 A_Scream
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_SpawnItemEx ("PoorBPinkyLostHisArm",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0)
        Stop
		
		LeftDeath:
		SRG8 A 8
		SRG8 B 8 A_Scream
		SRG8 C 4
		SRG8 D 4 A_NoBlocking
		SRG8 E 4
        TNT1 A 0 A_SpawnItem ("DeadBDemon1Left", 1)
		TNT1 A 0 A_SpawnItem ("GrowingBloodPool")
		Stop
		
		CleanDeath:
		TNT1 A 0
		TNT1 A 0 A_Jump(96, "LeftDeath")
		SRG3 A 8
		SRG3 B 8 A_Scream
		SRG3 C 4
		SRG3 D 4 A_NoBlocking
		SRG3 E 4
		TNT1 A 0 A_SpawnItem ("GrowingBloodPool")
        TNT1 A 0 A_SpawnItem ("DeadBDemon1", 1)
		Stop
		
		Death.Kick:
		Death.Melee:
		Death.SuperKick:
		TNT1 A 0
		TNT1 A 0 A_FaceTarget
		TNT1 A 0 A_Recoil(5)
		TNT1 A 0 ThrustThingZ(0,20,0,1)
		Goto CleanDeath
		
		Death.Shotgun:
		TNT1 A 0
		TNT1 A 0 A_JumpIfCloser(300, "Death.Strong")
		Goto Death //+4
		
		Death.Strong:
		TNT1 A 0
		TNT1 A 0 A_Jump(128, "DeathRemoveArm")
		TNT1 A 0 A_FaceTarget

		Death.Strong2:
		 TNT1 AA 0 A_CustomMissile ("SmallBrainPiece", 60, 0, random (0, 360), 2, random (0, 160))
		 TNT1 AA 0 A_CustomMissile ("XDeath3", random (45, 55), random (5, -5), random (160, 200), 2, random (-10, 10))
		 TNT1 AA 0 A_CustomMissile ("XDeath2", random (45, 55), random (5, -5), random (160, 200), 2, random (-10, 10))
		TNT1 AAAAAA 0 A_CustomMissile ("BloodMistBig", 40, 0, random (0, 360), 2, random (40, 90))
		TNT1 A 0 A_CustomMissile ("BloodMistExtraBig", 40, 0, random (0, 360), 2, random (40, 90))
		TNT1 A 0 A_CustomMissile ("PinkyHeadPiece", 52, 0, random (0, 360), 2, random (0, 160))
		//TNT1 A 0 A_CustomMissile ("PinkyJaw", 52, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAAA 0 A_CustomMissile ("SmallBrainPieceFast", random (45, 55), random (5, -5), random (170, 190), 2, random (-10, 10))

		SRG5 AAAAAAA 2 A_CustomMissile ("Brutal_LiquidBlood", 40, 0, random (0, 360), 2, random (30, 110))
		SRG5 BBBBBB 2 A_CustomMissile ("Brutal_LiquidBlood", 40, 0, random (0, 360), 2, random (30, 110))
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_Scream 
		SRG5 CCCCCC 2 A_CustomMissile ("Brutal_LiquidBlood", 30, 0, random (0, 360), 2, random (30, 110))
		TNT1 A 0 A_SpawnItem ("GrowingBloodPool")
		SRG5 DDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDD 2 A_CustomMissile ("Brutal_LiquidBlood", 20, 0, random (0, 360), 2, random (30, 110))
		TNT1 A 0 A_SpawnItem("DeadBDemonNoHead")
		TNT1 A -1
        Stop


Death.ChainSaw:
		TNT1 A 0 A_SpawnItemEx("HealthPlusFatalityBonus", 0, 0, 20, random (-1, 1), random (-1, 1), 8)
		TNT1 A 0 A_SpawnItemEx("ArmorShard", 0, 0, 20, random (-1, 1), random (-1, 1), 8)	
		TNT1 A 0 A_SpawnItemEx("HealthPlusFatalityBonus", 0, 0, 20, random (-1, 1), random (-1, 1), 8)
		TNT1 A 0 A_SpawnItemEx("ArmorGem", 0, 0, 20, random (-1, 1), random (-1, 1), 8)	
		Death.Saw:
			TNT1 A 0
			TNT1 AA 0 A_SpawnItem("muchblood3", 0, 40)
			TNT1 A 0 A_Jump(126, "DeathRemoveArm")
        TNT1 A 0 A_CustomMissile ("MuchBlood", 50, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_CustomMissile ("BloodDemonArm", 35, 0, random (0, 360), 2, random (0, 160))
TNT1 A 0 A_CustomMissile ("BloodMistExtraBig", 40, 0, random (0, 360), 2, random (40, 90))

			TNT1 A 0 A_CustomMissile ("XDeathStomach", 60, 0, random (0, 360), 2, random (0, 160))
			TNT1 AA 0 A_CustomMissile ("XDeath2b", 60, 0, random (0, 360), 2, random (0, 160))
			TNT1 AA 0 A_CustomMissile ("XDeath3b", 60, 0, random (0, 360), 2, random (0, 160))
			SRG4 A 8
			SRG4 B 8 A_Scream
			SRG4 C 4 A_NoBlocking
			SRG4 DDEEFF 2 A_CustomMissile ("Brutal_LiquidBlood", 30, 0, random (0, 360), 2, random (30, 110))
			TNT1 A 0 A_SpawnItem ("GrowingBloodPool")
			TNT1 A 0 A_SpawnItem ("DeadBDemonHalf")
			TNT1 TTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTTT 2 A_CustomMissile ("Brutal_LiquidBlood", 20, 0, random (0, 360), 2, random (30, 110))
			TNT1 T -1
			Stop


Death.ChainCut:
		TNT1 A 0 A_SpawnItemEx("HealthPlusFatalityBonus", 0, 0, 20, random (-1, 1), random (-1, 1), 8)
		TNT1 A 0 A_SpawnItemEx("ArmorShard", 0, 0, 20, random (-1, 1), random (-1, 1), 8)	
		TNT1 A 0 A_SpawnItemEx("HealthPlusFatalityBonus", 0, 0, 20, random (-1, 1), random (-1, 1), 8)
		TNT1 A 0 A_SpawnItemEx("ArmorGem", 0, 0, 20, random (-1, 1), random (-1, 1), 8)			
		Death.Cut:
			TNT1 A 0 A_Scream
			TNT1 A 0 A_NoBlocking
			TNT1 AA 0 A_SpawnItem("muchblood3", 0, 40)
			TNT1 A 0 A_Jump(200, "Death.Cut2")
		//	TNT1 A 0 A_CustomMissile ("XDeathHalfDemon", 60, 0, random (0, 360), 2, random (0, 160))
			TNT1 A 0 A_CustomMissile ("XDeathStomach", 60, 0, random (0, 360), 2, random (0, 160))
			TNT1 AA 0 A_CustomMissile ("XDeath1", 60, 0, random (0, 360), 2, random (0, 160))
			TNT1 AA 0 A_CustomMissile ("XDeath2b", 60, 0, random (0, 360), 2, random (0, 160))
			TNT1 AA 0 A_CustomMissile ("XDeath3b", 60, 0, random (0, 360), 2, random (0, 160))
			Srg9 AAABBBCCC 3 A_CustomMissile ("Brutal_LiquidBlood", 35, 0, random (0, 360), 2, random (30, 110))
			TNT1 A 0 A_SpawnItem ("GrowingBloodPool")
			Srg9 CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC 2 A_CustomMissile ("Brutal_LiquidBlood", 15, 0, random (0, 360), 2, random (30, 110))
			Srg9 C 0 A_SpawnItem("DeadBDemonHalf23")
