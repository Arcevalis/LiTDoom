
Actor ImpExecute1
{
Health 150
Radius 45
Height 4
Mass 99999
Painchance 256
Species "Marines"
+ISmonster
+SHOOTABLE
+FORCEXYBILLBOARD
+LOOKALLAROUND

DamageFactor "Fall", 0.0 DamageFactor "Trample", 0.0 DamageFactor "headkick", 0.0
Damagefactor "kick", 50.0
Damagefactor "Fatality", 50.0
damagefactor "use", 0.0 damagefactor "stealth", 0.0
	States
	{
	Spawn:
		TNT1 A 0 
		IZD4 MMMM 1 
		TNT1 A 0 a_Jumpifinventory("wallExecutiontoken",1,"Vanish")
		TNT1 A 0 a_Jumpifinventory("axefinisher2",1,"Split")
		TNT1 A 0 a_Jumpifinventory("beheadingtoken",1,"Stunned")
		TNT1 A 0 a_Jumpifinventory("axefinisher",1,"Stunned")
		13EI A 60
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		NULL A 0 A_SpawnItem ("StompedImp")
		Stop
		
	Pain:
		TNT1 A 0
		TNT1 A 0 a_Jumpifinventory("beheadingtoken",1,"Homerun")
		TNT1 A 0 a_Jumpifinventory("axefinisher",1,"Maim")
		TNT1 A 0 a_Jumpifinventory("axefinisher2",1,"Split2")
		TNT1 A 0 A_startSound("imp/pain",167,0,0.7)
		TNT1 A 0 A_SpawnItem("Blood", 0, 1)
		13EI B 3
		13EI C 60
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		NULL A 0 A_SpawnItem ("StompedImp")
		Stop
		
		
	Death:
		TNT1 A 0 a_Jumpifinventory("beheadingtoken",1,"Homerun")
		TNT1 A 0 a_Jumpifinventory("axefinisher",1,"Maim")
		PUFF A 0 A_startsound("KICKGORE", 80,0,1.0,0,0)
		13EI C 1
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_SpawnItem("ImpHeadExplode")
		TNT1 A 0 A_SpawnItemEx("SplatteredSmall", 0, 15)
		TNT1 A 0 A_GiveInventory("BootsSmearedWithRedBlood", 6)
		13EI B 8
		13EI D 15
		NULL A 0 A_SpawnItem ("StompedImp")
	Vanish:
		TNT1 A 0 A_SpawnItemEx("HealthPlusFatalityBonus", 0, 0, 0, random(-5,5), random(-5,5), 5)
		TNT1 A 0 A_SpawnItemEx("armorgem", 0, 0, 0, random(-5,5), random(-5,5), 5,0,SXF_NOCHECKPOSITION,180)	
		Stop
		
	Death.Fatality:
		TNT1 A 0 a_Jumpifinventory("beheadingtoken",1,"Homerun")
		TNT1 A 0 a_Jumpifinventory("axefinisher",1,"Maim")
		PUFF A 0 A_startsound("KICKGORE", 80,0,1.0,0,0)
		13EI C 1
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
		TNT1 AAAA 0 A_SpawnItem("Blood", 0, 1)
		13EI C 1 A_FaceTarget
		TNT1 A 0 A_Recoil(13)
		TNT1 A 0 ThrustThingZ(0,20,0,1)
		TNT1 A 0 A_startSound("imp/pain",167,0,0.7)
		TNT1 A 0 A_CustomMissile ("MuchBlood2", 15, 0, random (0, 360), 2, random (0, 160))
		TNT1 AA 0 A_CustomMissile ("XDeath1", 15, 0, random (0, 360), 2, random (0, 160))
		TNT1 AAAAAA 0 A_CustomMissile ("Teeth", 15, 0, random (0, 360), 2, random (0, 160))
        TR06 ABCDE 4 A_CustomMissile ("Brutal_LiquidBlood3", 15, 0, random (0, 360), 2, random (0, 40))
		TR06 FGHIJK 4 A_JumpIf((MomY == 0), "DSFStop")
		TNT1 A 0 A_SpawnItemEx("HealthPlusFatalityBonus", 0, 0, 0, random(-5,5), random(-5,5), 25)
		TNT1 A 0 A_SpawnItemEx("armorgem", -30, 0, 0, random(-5,5), random(-5,5), 5,0,SXF_NOCHECKPOSITION,180)
		Goto DSFStop+1
		
	DSFStop:
		//TNT1 A 0 A_SpawnItem("Ploft3")
		TNT1 A 0 A_SpawnItem ("GrowingBloodPool")
		TNT1 A 0 A_CustomMissile ("SuperWallRedBlood", 2, 0, random (170, 190), 2, random (0, 0))
		TNT1 A 0 A_SpawnItem("DeadImpTR06K")
		Stop
	
	Split: 
		IZD4 MMMMMMM 1 
		13EI A 0 a_changeflag("flatsprite",1)
		13EI A 0 a_SETANGLE(ANGLE-220)
		13EI B 70
	Split2:		
		TNT1 A 0 A_startSound("imp/pain",19,0,0.5)
		TNT1 A 0 A_SpawnItem("Blood", 0, 1)
		TNT1 AA 0 A_CustomMissile ("XDeath1", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_Spawnitemex ("XDeathOrgan1", 5, -10,2,-1,-1,2)
		 TNT1 A 0 A_spawnitemex ("XDeathOrgan2",  -5, -10,2,-1,-1,2 )
		13EI B 3
		13EI C 51
		TNT1 A 0 A_SpawnItemEx("SplatteredSmall", 0, -15)
		TNT1 AAA 0 A_CustomMissile ("XDeath1", 40, 0, random (0, 360), 2, random (0, 160))
        TROG AAAAA 0 A_SpawnItem ("MuchBlood", 0, 10)
		TNT1 A 0 A_SpawnItemEx("RipImp2", 20,0,1,0,0,0,160)
		TNT1 A 0 A_SpawnItemex ("DeadImp_Half2", 0,0,0,-1,-3,1,160)
		TNT1 A 15
		TNT1 AA 0 A_SpawnItemEx("HealthPlusFatalityBonus", 0, 0, 0, random(-1,0), random(-1,1), 5)
		TNT1 AA 0 A_SpawnItemEx("armorgem", 0, 0, 0, random(-1,0), random(-1,1), 5,0,SXF_NOCHECKPOSITION,120)
		Stop
	
	Stunned:
		TNT1 A 0  a_setmass(1)
		TNT1 A 0 A_recoil(-5)
		TNT1 A 0 a_setsize(-1, 45)
		TNT1 A 0 A_ChangeFlag("noclip", 1)
		IZD4 M 1 
		IZD4 NO 4 
		TROH Q 1 a_stop
		13EI A 0 a_SETANGLE(ANGLE+180)
		TNT1 A 0 A_CustomMissile ("Brutal_LiquidBlood2", 30, 0, random (0, 360), 2, random (10, 90))	
		PUFF A 0 A_STARTSound("player/cyborg/fleshfist", 256)
		TNT1 A 0 A_startSound("imp/pain",167,0,0.7)
		TNT1 A 0 A_ChangeFlag("noclip", 0)
		TNT1 A 0  a_setmass(99999)
		TROO H 2
		13EI A 0 a_SETANGLE(ANGLE+180)
		TROH S 23
		TROH T 45
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		NULL A 0 A_SpawnItem ("StompedImp")
		Stop
		
	Homerun:
		TNT1 A 0
		TROH AAAA 2 A_CustomMissile ("Brutal_LiquidBlood2", 50, 0, random (0, 360), 2, random (60, 120))
		TNT1 A 0 A_SpawnItemEx("HealthPlusFatalityBonus", 0, 0, 0, random(-5,5), random(-5,5), 5)
		TNT1 A 0 A_SpawnItemEx("armorgem", 0, 0, 0, random(-5,5), random(-5,5), 5,0,SXF_NOCHECKPOSITION,120)
		TNT1 A 0 A_SpawnItem("ImpHeadExplode", 0, 40)
		TROH A 1 A_CustomMissile ("XDeathImpHeadFAST", 45, 0, 0, 2, 20)
		TNT1 A 0 A_SpawnItem("DyingImpNoHeadLikeChicken")
		TNT1 A 0 A_NoBlocking
		Stop
        TNT1 A 0 A_PlaySound("misc/xdeath2c")
		TROH FFFGGGHHHIII 2 A_CustomMissile ("Brutal_LiquidBlood2", 30, 0, random (0, 360), 2, random (10, 90))	
		        TNT1 A 0 A_PlaySound("misc/xdeath2c")
		TROH FFFGGGHHHIII 2 A_CustomMissile ("Brutal_LiquidBlood2", 30, 0, random (0, 360), 2, random (10, 90))		
		        TNT1 A 0 A_PlaySound("misc/xdeath2c")
		TROH FFFGGGHHHIII 2 A_CustomMissile ("Brutal_LiquidBlood2", 30, 0, random (0, 360), 2, random (10, 90))		
		        TNT1 A 0 A_PlaySound("misc/xdeath2c")
		TROH FFFGGGHHHIII 2 A_CustomMissile ("Brutal_LiquidBlood2", 30, 0, random (0, 360), 2, random (10, 90))		
				        TNT1 A 0 A_PlaySound("misc/xdeath2c")
		TROH FFFGGGHHHIII 2 A_CustomMissile ("Brutal_LiquidBlood2", 30, 0, random (0, 360), 2, random (10, 90))	
				        TNT1 A 0 A_PlaySound("misc/xdeath2c")
		TROH FFFGGGHHHIII 2 A_CustomMissile ("Brutal_LiquidBlood2", 30, 0, random (0, 360), 2, random (10, 90))
		TROH CCCCCCCC 2 A_CustomMissile ("Brutal_LiquidBlood2", 30, 0, random (0, 360), 2, random (60, 120))
		TROH DDD 2 A_CustomMissile ("Brutal_LiquidBlood2", 20, 0, random (0, 360), 2, random (60, 120)) 
		TNT1 A 0 A_SpawnItem ("GrowingBloodPool")
		TROH EEEEEEEEEEEEEEEEEEEEEEEEEEEEE 2 A_CustomMissile ("Brutal_LiquidBlood2", 15, 0, random (0, 360), 2, random (15, 40))
        TNT1 A 0 A_SpawnItem("DeadImp_NoHead")
		TNT1 A -1
		Stop
		
	Maim:   
		//leg
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_SpawnItemEx("XDeathImpLeg", 0,0,0,0,-5,2)
		TNT1 A 0 A_PlaySound("imp/pain")
		TNT1 A 0 A_changeflag("INVULNERABLE",1)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 32,0)
        TR09 DD 4 A_CustomMissile ("Brutal_LiquidBlood", 12, 0, random (0, 360), 2, random (0, 45))
        TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 32,0)
        TR09 D 5 A_CustomMissile ("Brutal_LiquidBlood", 12, 0, random (0, 360), 2, random (0, 45))
		TNT1 A 0 A_PlaySound("imp/pain")
        TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 32,0)
        TR09 FF 4 A_CustomMissile ("Brutal_LiquidBlood", 12, 0, random (0, 360), 2, random (0, 45))
        TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 32,0)
        TR09 F 2 A_Pain	
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 32,0)
        TR09 DD 4 A_CustomMissile ("Brutal_LiquidBlood", 12, 0, random (0, 360), 2, random (0, 45))
		TR09 FF 4 A_Pain
		
		//head
		TNT1 A 0 A_SpawnItem("ImpHeadExplode", 0, 30)
		TNT1 A 0 A_XScream
		TNT1 A 0 A_SpawnItemEx("XDeathImpHead", 0, 0, 30, 2, 5,5)
        TR09 I 20
        TNT1 A 0 A_SpawnItem ("DeadImp_Noleg", 1)	
		TNT1 AA 0 A_SpawnItemEx("HealthPlusFatalityBonus", -20, 0, 0, random(-1,1), random(-1,1), 5)
		TNT1 AA 0 A_SpawnItemEx("armorgem", -20, 0, 0, random(-1,1), random(-1,1), 5,0,SXF_NOCHECKPOSITION,120)
