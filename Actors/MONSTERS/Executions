		TNT1 A 0 A_SpawnItem ("GrowingBloodPool")
		TNT1 A 0 A_CustomMissile ("SuperWallRedBlood", 2, 0, random (170, 190), 2, random (0, 0))
		POS4 T 0
		TNT1 A 0 A_SpawnItem("DeadZombiemanShotguntotheFace2")
		Stop
		
	Stunned:
		TNT1 A 0  a_setmass(1)
		TNT1 A 0 A_recoil(-5)
		TNT1 A 0 a_setsize(-1, 45)
		TNT1 A 0 A_ChangeFlag("noclip", 1)
		FS11 S 1 
		FS11 TT 4 
		FS11 T 1 a_stop
		TNT1 A 0 A_CustomMissile ("Brutal_LiquidBlood2", 30, 0, random (0, 360), 2, random (10, 90))	
		PUFF A 0 A_STARTSound("player/cyborg/fleshfist", 256)
		TNT1 A 0 A_startSound("grunt/pain",167,0,0.7)
		TNT1 A 0 A_ChangeFlag("noclip", 0)
		TNT1 A 0  a_setmass(99999)
		13EI A 0 a_SETANGLE(ANGLE+180)
		SCZA G 2
		13EI A 0 a_SETANGLE(ANGLE+180)
		TNT1 A 0 A_spawnitemex("AXEDROPED", 5,0,20,0,-5,2)
		FSMS H 2
		FSMS Z 66
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		NULL A 0 A_SpawnItem ("Stompedlabguy")
		Stop
		
	Split: 
		LGUY A 7 
		13EI A 0 a_changeflag("flatsprite",1)
		13EI A 0 a_SETANGLE(ANGLE-220)
		LGUY A 70
	
		TNT1 A 0 A_startSound("grunt/pain",167,0,0.7)
		TNT1 A 0 A_SpawnItem("Blood", 0, 1)
		TNT1 AA 0 A_CustomMissile ("XDeath1", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_Spawnitemex ("XDeathOrgan1", 5, -10,2,-1,-1,2)
		 TNT1 A 0 A_spawnitemex ("XDeathOrgan2",  -5, -10,2,-1,-1,2 )
		 LGUY D 10
		 LGUY E 10
		 LGUY D 10
		 TNT1 A 0 A_startSound("grunt/pain",167,0,0.7)
		 LGUY E 10
		 LGUY D 10
		 LGUY E 6
		TNT1 A 0 A_SpawnItemEx("SplatteredSmall", 0, -15)
		TNT1 AAA 0 A_CustomMissile ("XDeath1", 40, 0, random (0, 360), 2, random (0, 160))
        TROG AAAAA 0 A_SpawnItem ("MuchBlood", 0, 10)
		TNT1 A 0 A_SpawnItemEx("RipLabguy2", 5,55,0,-1,-2,1,160)
		TNT1 A 0 A_SpawnItemex ("DeadLabguyFSD1H", 0,0,0,-1,-2,2,160)
		TNT1 A 15
		TNT1 AA 0 A_SpawnItemEx("HealthPlusFatalityBonus", 0, 0, 0, random(-1,0), random(-1,1), 5)
		TNT1 A 0 A_SpawnItemEx("bandages", -25, 0, 0, random(-1,0), random(-1,1), 5,0,SXF_NOCHECKPOSITION,0)
		Stop	
	
	Maim:   
		//leg
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_SpawnItemEx("XDeathSargeLeg1", 0,0,0,0,-5,2)
		TNT1 A 0 A_startSound("grunt/pain",167,0,0.7)
		TNT1 A 0 A_changeflag("INVULNERABLE",1)
		TNT1 A 0 A_ChangeFlag("SHOOTABLE", 0)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 32,0)
		TNT1 AA 0 A_CustomMissile ("Brutal_LiquidBlood", 12, 0, random (0, 360), 2, random (0, 45))
		FSS7 ABCD 1
        FSS7 E 4 A_CustomMissile ("Brutal_LiquidBlood", 12, 0, random (0, 360), 2, random (0, 45))
        TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 32,0)
        FSS7 D 5 A_CustomMissile ("Brutal_LiquidBlood", 12, 0, random (0, 360), 2, random (0, 45))
		TNT1 A 0 A_startSound("grunt/pain",167,0,0.7)
        TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 32,0)
        FSS7 EE 4 A_CustomMissile ("Brutal_LiquidBlood", 12, 0, random (0, 360), 2, random (0, 45))
        TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 32,0)
        FSS7 D 2 A_Pain	
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10", 0, 32,0)
       FSS7 EE 4 A_CustomMissile ("Brutal_LiquidBlood", 12, 0, random (0, 360), 2, random (0, 45))
		FSS7 DD 4 A_Pain
		
		//head
		TNT1 A 0 A_SpawnItem("GENERICHeadExplode", 0, 30)
		TNT1 A 0 A_XScream
		TNT1 A 0 A_SpawnItemEx("LABGUYHead", 0, 0, 30, 2, 5,5)
		FSSS IJ 4
        FSSS H 12
        TNT1 A 0 A_SpawnItem ("LABGUY_NoLeg2", 1)	
		TNT1 AA 0 A_SpawnItemEx("HealthPlusFatalityBonus", -25, 0, 0, random(-1,0), random(-1,1), 5)
		TNT1 A 0 A_SpawnItemEx("bandages", -25, 0, 0, random(-1,0), random(-1,1), 5,0,SXF_NOCHECKPOSITION)
		Stop
	
		HOMERUN:
		TNT1 A 0 A_CustomMissile ("LabguyHeadfast", 45, 0, 0, 2, 20)
		TNT1 A 0 A_PlaySound("misc/xdeath2c")
		FSAD FGH 4 A_SpawnItem ("Blood", 0, 56)
		TNT1 A 0 A_SpawnItemEx("HealthPlusFatalityBonus", 0, 0, 0, random(-5,5), random(-5,5), 5)
		TNT1 A 0 A_SpawnItemEx("chemspawner", 0, 0, 0, random(-5,5), random(-5,5), 5,0,0,180)
		TNT1 A 0 A_SpawnItemEx("bandages", -25, 0, 0, random(-1,0), random(-1,1), 5,0,SXF_NOCHECKPOSITION,120)
		TNT1 A 0 a_JUMP(127,"HOMERUN2")
		TNT1 A 0 A_PlaySound("misc/xdeath2c")
		FSAD FGH 4 A_SpawnItem ("Blood", 0, 56)
		TNT1 A 0 A_PlaySound("misc/xdeath2c")
		FSAD FGH 4 A_SpawnItem ("Blood", 0, 56)
		TNT1 A 0 A_PlaySound("misc/xdeath2c")
		FSAD FGH 4 A_SpawnItem ("Blood", 0, 56)
		TNT1 A 0 A_PlaySound("misc/xdeath2c")
		FSAD FGH 4 A_SpawnItem ("Blood", 0, 56)
		TNT1 A 0 A_PlaySound("misc/xdeath2c")
		FSAD FGH 4 A_SpawnItem ("Blood", 0, 56)
		TNT1 A 0 A_PlaySound("misc/xdeath2c")
		FSAD FGH 4 A_SpawnItem ("Blood", 0, 56)
		TNT1 A 0 A_PlaySound("misc/xdeath2c")
		TNT1 A 0 A_Custommissile("BloodyAxe", 32, 0, random (0, 360), 2, random (50, 130))
		FSAD ABBC 10 A_CustomMissile ("MuchBlood", 30, 0, random (0, 360), 2, random (0, 160))
		FSAD DE 6
		TNT1 A 0 A_SpawnItem ("GrowingBloodPool")
		TNT1 A 0 A_PlaySound("BODYF",6)
		TNT1 A 0 A_SpawnItem ("DeadLabguyFSADE")
		Stop

		HOMERUN2:
		TNT1 A 0
		TNT1 A 0 A_CustomMissile ("MuchBlood2", 40, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 A_Custommissile("BloodyAxe", 32, 0, random (0, 360), 2, random (50, 130))
		FSAD A 15 A_CustomMissile ("MuchBlood", 40, 0, random (0, 360), 2, random (0, 160))
		FSAD B 9 A_XScream
		FSAD C 9 A_NoBlocking
		FSAD DE 5
		TNT1 A 0 A_SpawnItem ("GrowingBloodPool")
		TNT1 A 0 A_PlaySound("BODYF",6)
		TNT1 A 0 A_SpawnItem ("DeadLabguyFSADE")
		Stop
	
		}
