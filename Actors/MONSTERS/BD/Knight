ACTOR HellKnight2 : HellKnight 30077
{
    Scale 1.0
    Speed 8
    Health 500
	Painchance "Bleeding", 256
	PainChance "Acid", 255
	DamageFactor "Explosive", 1.2							  
    damagefactor "Crush", 10.0
	damagefactor "GibRemoving", 0.0 PainChance "HangingHook", 0
	damagefactor "Shotgun", 0.5	
	damagefactor "bullet", 1.5
	Painchance "Shock", 256
	PainChance "Kick", 255 PainChance "Leg", 255
	PainChance "ExtremePunches", 255 PainChance "SuperPunch", 255
	PainChance "LowKick", 220
	PainChance "Head", 200
	Painchance "beartrap", 255
	PainChance "Explosive", 255
	Painchance "bleeding", 200 painchance "bleedingarmor", 140
	PainChance 25
	PainChance "ChainSaw", 200
	PainChance "ChainCut", 200
	PainChance "Saw", 190
	PainChance "Cut", 100
	Painchance "minigun", 80
	damagefactor "Fatality", 2.0
	damagefactor "Chaincut", 0.8 damagefactor "chainsaw", 0.8
	DamageFactor "CauseSplash", 0.0 DamageFactor "CauseObjectsToSplashSlime", 0.0 DamageFactor "CauseObjectsToSplashNukage", 0.0
	DamageFactor "CauseObjectsToSplashBlood", 0.0 DamageFactor "CauseObjectsToSplashLava", 0.0
	damagefactor "SSG", 0.5	damagefactor "Avoid", 0.0	damagefactor "TeleportRemover", 0.0
	damagefactor "head", 0.8
	DamageFactor "Use", 0.0
	DamageFactor "Stealth", 0.0
	PainSound "PSXDPN"
	Game Doom
	DeathHeight 0
	BurnHeight 0
	Height 56
	Radius 16
    GibHealth 30
	-BOSSDEATH
	-FASTER
	MaxStepHeight 24
	MaxDropOffHeight 32
	damagefactor "Avoid", 0.0
	damagefactor "Blood", 0.0 damagefactor "BlueBlood", 0.0 damagefactor "GreenBlood", 0.0
    BloodType "Green_Blood", "GreenSawBlood", "GreenSawBlood"
	BloodColor "DarkGreen"
	DropItem "DemonStrengthRune" 2
	Obituary "%o was slain by a Hell Knight."
	DamageType HKF
	Species "Bruisers"
	+NOINFIGHTSPECIES
	+DONTHARMSPECIES
	Tag "Hell Knight"
    MeleeDamage 5
	States
	{
		
		////SPAWNOPTIONS
ReplaceVanilla:
		TNT1 A 0 
		goto stand
	HellspawnOFC:
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
		TNT1 A 0 A_ChangeFlag("COUNTKILL", 0)
		TNT1 A 0 A_SpawnItemEx ("BruiserDemon",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERPOINTERS ,0, tid)
		Stop
	HellspawnMaybe:
		TNT1 A 0 A_Jump(160, "Stand")
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
		TNT1 A 0 A_ChangeFlag("COUNTKILL", 0)
		TNT1 A 0 A_SpawnItemEx ("BruiserDemon",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERPOINTERS ,0, tid)
		Stop
		
	SpawnNospace:
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
		TNT1 A 0 A_ChangeFlag("COUNTKILL", 0)
		TNT1 A 0 A_SpawnItemEx ("BuffBullDemon",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERPOINTERS ,0, tid)
		Stop
	
    Spawn:
		BOS2 B 0
		//
		TNT1 A 0 A_Checkceiling("SpawnnoSpace")
		TNT1 A 0 A_JUMPIF(GETCVAR("monsterspawns")==4, "ReplaceVanilla")
		TNT1 A 0 ACS_NamedExecuteAlways("BruiserOFC")
		TNT1 A 0 ACS_NamedExecuteAlways("BruiserMaybe")
		TNT1 A 0 ACS_NamedExecuteAlways("BDCHeckClassicMonsters", 0, 0, 0, 0)//Check if Vanilla Mode is activated
		//
		TNT1 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")
		TNT1 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")
		TNT1 A 0 ACS_NamedExecuteWithResult("RADARSIGNAL",0)
		BOS2 B 1
		
	Givearmor:
		TNT1 A 0 A_JumpIf(GetCVAR("EnemyArmorValue")==true,"Givearmor2")
		TNT1 A 0 A_GiveInventory("60armorlow")
		TNT1 A 0 A_GiveInventory("hasmediumarmor")
		goto stand
	Givearmor2:	
		TNT1 A 0 A_GiveInventory("60armormed")
		TNT1 A 0 A_GiveInventory("hasmediumarmor")
		Goto Stand
		
	//////
	Stand:
		TNT1 A 0 A_CheckSight("Stand2")
		TNT1 A 0 A_SpawnItem("HeadshotTarget20b", 12, 56,0)
		BOS2 BB 5 A_Look
		Loop
	
	Stand2:
		BOS2 B 11 A_Look
		Goto Stand
		
	See: 
	BOS2 B 0
	TNT1 A 0 A_CheckSight("SeeNeverSeen")
	Goto Rage	
	See2:
		TNT1 A 0
		TNT1 A 0 A_TakeInventory("EnemyMemory", 15)
		TNT1 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")

	See3:
	
	TNT1 A 0 A_CheckSight("SeeNeverSeen")
	
	SeeContinue:
		
		//TNT1 A 0 A_SpawnItemEx("FootSep", 0, 0, 20, 0, 0)
		TNT1 A 0 A_JumpIfInventory("EnemyMemory", 20, "ForgetTarget")
		TNT1 A 0 A_GiveInventory("EnemyMemory", 1)
		
		TNT1 A 0 A_SpawnItem("HeadshotTarget6b", 18, 56,0)
		BOS2 AAA 2 A_Chase
		
		TNT1 A 0 A_SpawnItem("HeadshotTarget6b", 18, 56,0)
		BOS2 BBb 2 A_Chase
		
		TNT1 A 0 A_SpawnItem("HeadshotTarget6b", 18, 56,0)
		BOS2 CCc 2 A_Chase
		
		TNT1 A 0 A_SpawnItem("HeadshotTarget6b", 18, 56,0)
		BOS2 DDD 2 A_Chase
		
		TNT1 A 0 A_JumpIfInTargetInventory("IsInvisible", 1, "ForgetTarget")
		TNT1 A 0 A_JumpIfintargetInventory("predatormode", 1, "ForgetTarget")
		Loop
	
	
	SeeNeverSeen:
        //TNT1 A 0 A_SpawnItemEx("FootSep", 0, 0, 20, 0, 0)
		
		TNT1 A 0 A_SpawnItem("HeadshotTarget4b", 18, 56,0)
		BOS2 AA 2 A_Chase
		TNT1 A 0 A_SpawnItem("HeadshotTarget4b", 18, 56,0)
		BOS2 AA 2 A_Chase
		
		
		TNT1 A 0 A_SpawnItem("HeadshotTarget4b", 18, 56,0)
		BOS2 BB 2 A_Chase
		TNT1 A 0 A_SpawnItem("HeadshotTarget4b", 18, 56,0)
		BOS2 BB 2 A_Chase
		
		TNT1 A 0 A_SpawnItem("HeadshotTarget4b", 18, 56,0)
		BOS2 CC 2 A_Chase
		TNT1 A 0 A_SpawnItem("HeadshotTarget4b", 18, 56,0)
		BOS2 CC 2 A_Chase
		
		TNT1 A 0 A_SpawnItem("HeadshotTarget4b", 18, 56,0)
		BOS2 DD 2 A_Chase
		TNT1 A 0 A_SpawnItem("HeadshotTarget4b", 18, 56,0)
		BOS2 DD 2 A_Chase
		TNT1 A 0 A_JumpIfInTargetInventory("IsInvisible", 1, "ForgetTarget")
		Loop
   RAGE:
        TNT1 A 0
		TNT1 A 0 A_CheckSight("See2")
        BTS2 ABC 1
		BTS2 DEDF 2
		BTS2 DEDF 2
		BTS2 CBA 1 A_FaceTarget
		TNT1 A 0 HealThing(200)
		TNT1 A 0 A_Jump(96, "MissileInRage")
		Goto See2
		
	Idle:
	LookingForPlayer:
	    TNT1 A 0 A_GiveInventory("SKImp", 1)
	    TNT1 A 0 A_SpawnItem ("HeadshotTarget10b", 18, 56,0)
		BOS2 B 10 A_Look
        TNT1 A 0 A_SpawnItem ("HeadshotTarget10b", 18, 56,0)
		BOS2 B 10 A_Look
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10b", 18, 56,0)
		BOS2 B 10 A_Look
		TNT1 A 0 A_SetAngle(angle - 40)
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10b", 18, 56,0)
		BOS2 B 10 A_Look
        TNT1 A 0 A_SpawnItem ("HeadshotTarget10b", 18, 56,0)
		BOS2 B 10 A_Look
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10b", 18, 56,0)
		BOS2 B 10 A_Look
		TNT1 A 0 A_SetAngle(angle + 40)
        TNT1 A 0 A_SpawnItem ("HeadshotTarget10b", 18, 56,0)
		BOS2 B 10 A_Look
		TNT1 A 0 A_SpawnItem ("HeadshotTarget10b", 18, 56,0)
		BOS2 B 10 A_Look
        TNT1 A 0 A_SpawnItem ("HeadshotTarget10b", 18, 56,0)
		BOS2 B 10 A_Look
		
		BOS2 A 0 A_Wander
		BOS2 AA 2 A_Look
		TNT1 A 0 A_Recoil(-2)
		TNT1 A 0 A_SpawnItem("HeadshotTarget6b", 18, 56,0)
		
		BOS2 B 0 A_Wander
		BOS2 BB 2 A_Look
		TNT1 A 0 A_Recoil(-2)
		TNT1 A 0 A_SpawnItem("HeadshotTarget6b", 18, 56,0)
		
		BOS2 C 0 A_Wander
		BOS2 CC 2 A_Look
		TNT1 A 0 A_Recoil(-2)
		TNT1 A 0 A_SpawnItem("HeadshotTarget6b", 18, 56,0)
		
		BOS2 D 0 A_Wander
		BOS2 DD 2 A_Look
		TNT1 A 0 A_Recoil(-2)
		TNT1 A 0 A_SpawnItem("HeadshotTarget6b", 18, 56,0)
		
		
		TNT1 A 0 A_jump(128, "LookingForPlayer")
		
		
		BOS2 A 0 A_Wander
		BOS2 AA 2 A_Look
		TNT1 A 0 A_Recoil(-2)
		TNT1 A 0 A_SpawnItem("HeadshotTarget6b", 18, 56,0)
		
		BOS2 B 0 A_Wander
		BOS2 BB 2 A_Look
		TNT1 A 0 A_Recoil(-2)
		TNT1 A 0 A_SpawnItem("HeadshotTarget6b", 18, 56,0)
		
		BOS2 C 0 A_Wander
		BOS2 CC 2 A_Look
		TNT1 A 0 A_Recoil(-2)
		TNT1 A 0 A_SpawnItem("HeadshotTarget6b", 18, 56,0)
		
		BOS2 D 0 A_Wander
		BOS2 DD 2 A_Look
		TNT1 A 0 A_Recoil(-2)
		TNT1 A 0 A_SpawnItem("HeadshotTarget6b", 18, 56,0)
		Goto LookingForPlayer
		
		
		
	ForgetTarget:
		TNT1 A 0 A_ClearTarget
		TNT1 A 0 A_TakeInventory("EnemyMemory", 25)
		TNT1 A 0 A_GiveInventory("LookingForPlayer", 1)
		Goto LookingForPlayer	

Melee:
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
        BOS2 E 6 A_FaceTarget
        TNT1 A 0 A_PlaySound("weapons/gswing")
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		BOS2 F 6 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		TNT1 A 0 a_checkproximity("Parried","Axeparry",80.0,1,CPXF_NOZ)
		BOS2 G 6 A_CustomMissile("KnightAttack",10,0,0,0)
		TNT1 A 0 A_CustomMissile("Hellrupture",10,0,0,0)
TNT1 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")
		Goto See2


	Melee2:
		TNT1 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")
		
		BOS2 EF 8 A_FaceTarget
		TNT1 A 0 a_checkproximity("Parried","Axeparry",80.0,1,CPXF_NOZ)
		BOS2 G 8 A_CustomMissile("KnightAttack",10,0,0,0)
		TNT1 A 0 A_CustomMissile("Hellrupture",10,0,0,0)
		Goto See2
    Missile:
		TNT1 A 0
		TNT1 A 0 A_TakeInventory("EnemyMemory", 30)
		TNT1 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsACyberdemon", 1, "CheckRetreat")
		TNT1 A 0 A_JumpIfInTargetInventory("TargetIsAMastermind", 1, "CheckRetreat")
		TNT1 A 0 A_JumpIfInventory("StopKickingMyBallsMotherFucker", 1, "MissileInRage")
		TNT1 A 0 A_FaceTarget
		TNT1 A 0 A_SpawnItemEx("HeadshotTarget8b", 18, 4, 56)
		BOS2 E 3 A_FaceTarget
		BOS2 E 2 A_FaceTarget
		TNT1 A 0 a_checkproximity("Parried","Axeparry",80.0,1,CPXF_NOZ)
		TNT1 A 0 bright A_CustomMissile ("SmallGreenFlameTrails", 64, 30, random (0, 360), 2, random (70, 110))
		TNT1 A 0 A_CustomMissile ("BallGettingReadyGreen", 64, 30, random (0, 140), 2, random (0, 160))
		BOS2 E 2 A_FaceTarget
		TNT1 A 0 bright A_CustomMissile ("SmallGreenFlameTrails", 64, 30, random (0, 360), 2, random (70, 110))
		TNT1 A 0 A_CustomMissile ("BallGettingReadyGreen", 64, 30, random (0, 140), 2, random (0, 160))
		BOS2 E 2 A_FaceTarget
		TNT1 A 0 bright A_CustomMissile ("SmallGreenFlameTrails", 64, 30, random (0, 360), 2, random (70, 110))
		TNT1 A 0 A_CustomMissile ("BallGettingReadyGreen", 64, 30, random (0, 140), 2, random (0, 160))
		BOS2 E 2 A_FaceTarget
		TNT1 A 0 A_SpawnItemEx("HeadshotTarget8b", 18, 4, 56)
		TNT1 A 0 bright A_CustomMissile ("SmallGreenFlameTrails", 64, 30, random (0, 360), 2, random (70, 110))
		TNT1 A 0 A_CustomMissile ("BallGettingReadyGreen", 64, 30, random (0, 140), 2, random (0, 160))
		BOS2 E 2 A_FaceTarget
		TNT1 A 0 bright A_CustomMissile ("SmallGreenFlameTrails", 64, 30, random (0, 360), 2, random (70, 110))
		TNT1 A 0 A_CustomMissile ("BallGettingReadyGreen", 64, 30, random (0, 140), 2, random (0, 160))
		BOS2 E 2 A_FaceTarget
		TNT1 A 0 bright A_CustomMissile ("SmallGreenFlameTrails", 64, 30, random (0, 360), 2, random (70, 110))
		TNT1 A 0 A_CustomMissile ("BallGettingReadyGreen", 64, 30, random (0, 140), 2, random (0, 160))
		BOS2 E 2	
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		BOS2 F 8 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		BOS2 G 8 A_CustomMissile("BaronBall", 36, 0, 0, 1) 
		Goto See2
		
	MissileInRage:
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		TNT1 A 0 A_TakeInventory("CantFire", 1)
		TNT1 A 0 A_CustomMissile("MonsterTargetCheck", 34, 0, 0, 0) 
		TNT1 A 0 A_CustomMissile("MonsterTargetCheckFaster", 34, 0, 0, 0) 
		BOS2 E 3 A_FaceTarget
		TNT1 A 0 A_JumpIfInventory("CantFire",1, "SeeContinue")
		BOS2 E 3 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		BOS2 F 6 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		TNT1 A 0 a_checkproximity("Parried","Axeparry",80.0,1,CPXF_NOZ)
		BOS2 G 6 A_CustomMissile("BaronBall", 36, 0, 0, 1)
		
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		BAS2 E 6 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		BAS2 F 6 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		TNT1 A 0 a_checkproximity("Parried","Axeparry",80.0,1,CPXF_NOZ)
		BAS2 G 6 A_CustomMissile("BaronBall", 36, 0, 0, 1)

		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		BOS2 E 6 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		BOS2 F 6 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 18, 56,0)
		TNT1 A 0 a_checkproximity("Parried","Axeparry",80.0,1,CPXF_NOZ)
		BOS2 G 6 A_CustomMissile("BaronBall", 36, 0, 0, 1)
		Goto See2
		
	CheckRetreat:
	TNT1 A 0
	TNT1 A 0 A_JumpIfCloser(500, "Retreat")
	Goto Missile+5
	
	Retreat:
	TNT1 A 0
        //TNT1 A 0 A_SpawnItemEx("FootSep", 0, 0)
		TNT1 A 0 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 66,0)
		BOS2 DD 2 A_Recoil(2)
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 66,0)
		BOS2 DD 2 A_Recoil(2)
		TNT1 A 0 A_FaceTarget
		
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 66,0)
		BOS2 CC 2 A_Recoil(1)
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 66,0)
		BOS2 CC 2 A_Recoil(1)
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 66,0)
		BOS2 BB 2 A_Recoil(1)
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 66,0)
		BOS2 BB 2 A_Recoil(1)
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 66,0)
		BOS2 AA 2 A_Recoil(1)
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 66,0)
		BOS2 AA 2 A_Recoil(1)
		Goto Missile+5

pain.bleedingarmor:
Pain.Bleeding:
	TNT1 A 0 A_CustomMissile ("Green_FlyingBlood", 55, 0, random (0, 360), 2, random (0, 160))
	TNT1 A 0 a_Jump(127, "SeeContinue")
	goto Pain

	Pain.Shock:
	TNT1 A 0 A_spawnitem("plasmaexplosion",0,30)
	TNT1 A 0 A_giveinventory("shockcharge",7)
	TNT1 A 0 A_playsound("Sparks/Spawn", CHAN_7)
	goto pain
	
PAIN.SYRINGE2:
	TNT1 A 0 A_CHANGEFLAG("FORCEINFIGHTING",1)
	TNT1 A 0 A_CHANGEFLAG("DOHARMSPECIES",1)
	TNT1 A 0 A_CHANGEFLAG("DONTHARMSPECIES",0)
	TNT1 A 0 A_CHANGEFLAG("DONTHARMCLASS",0)
	TNT1 A 0 A_CHANGEFLAG("NOINFIGHTSPECIES",0)
	TNT1 A 0 A_CHANGEFLAG("NOINFIGHTING",0)
	TNT1 A 0 A_CLEARTARGET
	TNT1 A 0 A_EXPLODE(1,270,XF_NOTMISSILE | XF_EXPLICITDAMAGETYPE,0,270,0,0,"bulletpuff","frenzytrigger")
	goto see
PAIN.FRENZYTRIGGER:
	TNT1 A 0 Healthing(1)
	TNT1 A 0 A_CHANGEFLAG("FORCEINFIGHTING",1)
	TNT1 A 0 A_CHANGEFLAG("DOHARMSPECIES",1)
	TNT1 A 0 A_CHANGEFLAG("DONTHARMSPECIES",0)
	TNT1 A 0 A_CHANGEFLAG("DONTHARMCLASS",0)
	TNT1 A 0 A_CHANGEFLAG("NOINFIGHTSPECIES",0)
	TNT1 A 0 A_CHANGEFLAG("NOINFIGHTING",0)
  goto See
	
	PAIN.SYRINGE1:
	TNT1 A 0 A_CHANGEFLAG("FRIENDLY",1)
	tnt1 a 0 givebody(500)
	Pain.Acid:
	BOS2 H 3
	BOS2 H 3 A_Pain
	BOS2 H 3
	BOS2 H 3 A_Pain
	BOS2 H 3
	BOS2 H 3 A_Pain
	Pain:
        TNT1 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")
		TNT1 A 0 A_SpawnItem("HeadshotTarget4b", 18, 56,0)
	    TNT1 A 0 A_TakeInventory("StopKickingMyBallsMotherFucker", 1)
		BOS2 H 3
		TNT1 A 0 A_SpawnItem("HeadshotTarget4b", 18, 56,0)
		BOS2 H 3 A_Pain
		TNT1 A 0 A_JumpIfInventory ("shockcharge", 1, "Shocked")
	Goto See2
	
	Shocked:
	TNT1 A 0 A_takeinventory("shockcharge",1)
	TNT1 A 0 A_spawnitem("plasmashockBEEG",0,50)
	TNT1 A 0 A_DamageSelf(2,"plasma")
	goto Pain
	
	Pain.Explosive:
	TNT1 A 0
	TNT1 A 0 A_SpawnItemEx("MuchBlood2Green", 0, 0, 60)
	TNT1 A 0 ThrustThingZ(0,10,0,1)
	Goto Pain
	
	Pain.Extremepunches:
	Pain.SuperPunch:
	Pain.HeavyImpact:
		TNT1 A 0 A_FaceTarget
		TNT1 A 0 A_ChangeFLag("NOPAIN", 1)
		TNT1 A 0 ThrustThingZ(0,30,0,1)
		TNT1 A 0 A_Recoil(8)
		BOS2 H 3
		TNT1 A 0 A_SpawnItem("HeadshotTarget4b", 18, 56,0)
		BOS2 H 3 A_Pain
		TNT1 A 0 A_ChangeFLag("NOPAIN", 0)
		Goto See2
	
	
	Pain.Beartrap:
		TNT1 A 0 A_giveinventory("istrapped",1)
		TNT1 A 0 a_changeflag("DONTHRUST",1)
		TNT1 A 0 a_changeflag("NOPAIN",1)
		TNT1 A 0 A_CustomMissile ("GREEN_FlyingBlood", 10, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 a_spawnitem("beartraptriggered",0,0)
		BOS2 H 3 a_scream
		TNT1 A 0 
	Trapped:
		BOS2 B 35 
		TNT1 A 0 a_jump(160,3)
		BOS2 H 3 a_scream
		TNT1 A 0 A_CustomMissile ("GREEN_FlyingBlood", 10, 0, random (0, 360), 2, random (0, 160))
		TNT1 A 0 a_checkproximity(1, "beartraptriggered", 50,1)
		GOTO SEECONTINUE
		TNT1 A 0 a_jump(25, "breakfree")		
		TNT1 A 0 a_jump(100, "MISSILETrapped")	
		TNT1 A 0 a_checkproximity("Trapped", "beartraptriggered", 50,1)
		TNT1 A 0 a_changeflag("NOPAIN",0)
		TNT1 A 0 a_changeflag("DONTHRUST",0)
		TNT1 A 0 A_TAKEinventory("istrapped",1)
		goto seecontinue
		
	Breakfree:
		KNRJ D 5
		TNT1 A 0 A_PlaySound("CLAP", 1)
		TNT1 A 0 A_KILLCHILDREN("CRUSH",0,"BEARTRAPTRIGGERED")
		TNT1 A 0 A_SPAWNITEMEX("BEARTRAPRIP",0,0,0, 15)
		KNRJ C 17
		goto SEECONTINUE
	
	MISSILETrapped:
		TNT1 A 0 A_TakeInventory("EnemyMemory", 30)
		TNT1 A 0 A_FaceTarget
		TNT1 A 0 A_SpawnItemEx("HeadshotTarget8b", 18, 4, 56)
		BOS2 E 3 
		BOS2 E 2 A_FaceTarget
		TNT1 A 0 bright A_CustomMissile ("SmallGreenFlameTrails", 64, 30, random (0, 360), 2, random (70, 110))
