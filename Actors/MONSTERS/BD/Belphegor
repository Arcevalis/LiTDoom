
ACTOR Belphegor2 : BaronofHell 2544
{
	//$category "BD Monsters"
    Scale 1.2
	Game Doom
    GibHealth 25
	+NOICEDEATH
	PainChance "ChainCut", 200
	PainChance "ChainSaw", 200
	PainChance "Kick", 255
	PainChance "Acid", 255
	Painchance "Shock", 256
	Painchance "Bleeding", 250 painchance "bleedingarmor", 190
	damagefactor "Bullet", 1.3
	Painchance "beartrap", 255
	damagefactor "Avoid", 0.0
	DamageFactor "Crush", 20.0
	DamageFactor "Use", 0.0
	DamageFactor "Stealth", 0.0
	damagefactor "Head", 2.0
	damagefactor "Slug", 1.2
	damagefactor "GibRemoving", 0.0
	damagefactor "MINORHead", 1.0
	DamageFactor "CauseSplash", 0.0 DamageFactor "CauseObjectsToSplashSlime", 0.0 DamageFactor "CauseObjectsToSplashNukage", 0.0
	DamageFactor "CauseObjectsToSplashBlood", 0.0 DamageFactor "CauseObjectsToSplashLava", 0.0
	damagefactor "TeleportRemover", 0.0
    Health 3000
	Mass 1900		  
	damagefactor "Chaincut", 0.5 damagefactor "chainsaw", 0.5
	damagefactor "Blood", 0.0 damagefactor "BlueBlood", 0.0 damagefactor "GreenBlood", 0.0
	damagefactor "Explosive", 1.7
	Obituary "%o was no match for the Belphegor."
	BloodType "Green_Blood", "GreenSawBlood", "GreenSawBlood"
	BloodColor "DarkGreen"
	DropItem "acidbloodspawner" 210
	DropItem "alphabloodspawner" 210
	Dropitem "hornspawner" 255
	Dropitem "demonstrengthrune" 255
	MaxStepHeight 24
	MaxDropOffHeight 32
	+boss
	+DONTHARMSPECIES
	+MISSILEMORE
	DeathHeight 4
	BurnHeight 0
    Speed 8
	FastSpeed 12
	Species "Bruisers"
	Radius 24//61SpriteW(120%=73)
	Height 88//78SpriteH(120%=94)
	Tag "Belphegor"
	
	States
	{
	
	SpawnNospace:
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
		TNT1 A 0 A_ChangeFlag("COUNTKILL", 0)
		TNT1 A 0 A_SpawnItemEx ("BuffBullDemon",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERPOINTERS ,0, tid)
		Stop
		
	ReplaceVanilla:
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 1)
		TNT1 A 0 A_ChangeFlag("COUNTKILL", 0)
		TNT1 A 0 A_SpawnItemEx ("BaronofHell",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		Stop
	
	Forget:
		TNT1 A 0 a_cleartarget
		TNT1 A 0 a_look
		goto Spawn2
	
	Spawn:
		TNT1 A 0
		//
		TNT1 A 0 A_Checkceiling("SpawnnoSpace")
		//TNT1 A 0 A_JUMPIF(GETCVAR("monsterspawns")==4, "ReplaceVanilla")
		//
		TNT1 A 0 ACS_NamedExecuteWithResult("RADARSIGNAL",0)
	Givearmor:
		TNT1 A 0 A_JumpIf(GetCVAR("EnemyArmorValue")==true,"Givearmor2")
		TNT1 A 0 A_GiveInventory("60armorMED")
		TNT1 A 0 A_GiveInventory("hasmediumarmor")
		goto SPAWN2
	Givearmor2:	
		TNT1 A 0 A_GiveInventory("60armorhigh")
		TNT1 A 0 A_GiveInventory("hasMEDIUMarmor")
		Goto SPAWN2
	Spawn2:
		BOS3 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
		BOS3 B 10 A_Look
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
		BOS3 D 10 A_Look
		Loop
	See:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("Token1", 1, "See2")
		BOS3 AAAA 2 A_Chase
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
		BOS3 BBBB 2 A_Chase("","")
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
		BOS3 CCCC 2 A_Chase
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
		BOS3 BBBB 2 A_Chase
		TNT1 A 0 A_JumpIfintargetInventory("predatormode", 1, "Forget")
		Loop
		
	See2:
		B0S3 PPP 1 A_Chase("","")
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
		B0S3 QQQ 1 A_Chase("","")
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
		B0S3 RRR 1 A_Chase
		TNT1 A 0 A_JumpIfintargetInventory("predatormode", 1, "Forget")
		Loop	

pain.beartrap:
TNT1 A 0 a_spawnitem("beartraprip",0,0)	
goto see
		
	Pain.Taunt:
		TNT1 A 0
		TNT1 A 0 A_CHangeFlag("FASTER", 1)
		TNT1 A 0 A_CHangeFlag("NOPAIN", 1)
		TNT1 A 0 A_GiveInventory("Token1", 1)
		B0S3 IJKL 3
		TNT1 A 0 A_PlaySound("baron/see")
		B0S3 MNONMNOMN 3
		B0S3 LKJ 3
		Goto See2
	Melee:
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
        BOS3 F 8
        TNT1 A 0 A_PlaySound("weapons/gswing")
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
		BOS3 G 8 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
		TNT1 AAAA 0 A_CustomMissile("KnightAttack",10,0,0,0)
		BOS3 H 8 A_FaceTarget
		TNT1 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")
		Goto See
		
	Melee2:
		TNT1 AAAA 0 A_CustomMissile("KnightAttack",10,0,0,0)
		TNT1 A 0 A_PlaySound("weapons/gswing")
		BOS3 H 8 A_FaceTarget
        TNT1 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")
		Goto See
		
		
    Missile:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("Token1", 1, "MissileAngry")
		TNT1 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
		BOS3 F 8 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
		BOS3 G 8 A_FaceTarget
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
		TNT1 A 0 A_JumpIfCloser(150, "Melee2")
		BOS3 H 8 A_CustomMissile("BelphegorGreenPlasmaBall", 47, 0, 0, 1) 
		Goto See
		
	
	 MissileAngry:
		TNT1 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")
		TNT1 A 0 A_SpawnItem("HeadshotTarget8b", 0, 88,0)
