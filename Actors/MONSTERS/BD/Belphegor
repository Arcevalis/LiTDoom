	PAIN.SYRINGE1:
	TNT1 A 0 A_JUMPIFinventory("syringedonce",1,3)
	TNT1 A 0 A_JUMPIFHEALTHLOWER(900,2)
	TNT1 A 0 A_GIVEINVENTORY("syringedonce",1)
	goto pain.acid
	tnt1 a 0 givebody(2000)
	TNT1 A 0 A_CHANGEFLAG("FRIENDLY",1)
	Pain.Acid:
		TNT1 A 0 A_JumpIfInventory("Curbstomp_Marine", 1, "CurbstompMarine")
