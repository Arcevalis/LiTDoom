		TNT1 A 0 A_JumpIfHealthLower(1000, "Pain.Taunt")
		BOS3 E 3
		TNT1 A 0 A_SpawnItem("HeadshotTarget4b", 0, 88,0)
		BOS3 E 3 A_Pain
		TNT1 A 0 A_JumpIfInventory ("shockcharge", 1, "Shocked")
		TNT1 A 0 A_Jump(32, "Pain.Taunt")
		Goto See
	
	Shocked:
	TNT1 A 0 A_takeinventory("shockcharge",1)
	TNT1 A 0 A_spawnitem("plasmashockBEEG",0,60)
		TNT1 A 0 A_DamageSelf(2,"plasma")
	goto Pain
		
    Death.SSG:
	XDeath:
	death.RailgunBig:
	Death.Railgun:
		 TNT1 AA 0 A_CustomMissile ("XDeath2bGreen", 55, 0, random (0, 360), 2, random (20, 90))
