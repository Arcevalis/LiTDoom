// HEALTH  MILD
ACTOR HealStim : CustomInventory
{
	+INVBAR
+canpass
	+CANNOTPUSH
	Inventory.MaxAmount 6
	Inventory.Interhubamount 6
	Inventory.PickupMessage "$GOTMILDSTIM"
	Inventory.PickUpSound "STIMBAG"
	Tag "Healing stimulant"
	Inventory.Icon "BSYRB0"
	SCALE 0.3
	States
	{
		Spawn:
		BSYR A 1
		TNT1 A 0 
		
		Stay:
		BSYR A -1
		Stop
		
		FailDrug:
		TNT1 A 0 
		Fail
		
		Use:
		TNT1 A 0 a_jumpifinventory("choosingreward",1, "Faildrug")
		TNT1 A 0 a_jumpifinventory("readingammo",1, "Faildrug")
		TNT1 A 0 A_Giveinventory("battledrugtoken",1)
		TNT1 A 0 A_Giveinventory("bluedrug",1)
		
		TNT1 A 0 A_Takeinventory("usedstamina",75)
		TNT1 A 0 HEALTHING(25,300)		
		
		TNT1 A 1 A_playsound("DRUG1",3,1.0)
	}
}	

// HEALTH POTENT
