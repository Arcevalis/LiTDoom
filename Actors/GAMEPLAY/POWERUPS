
// MEDICAL POWERUPS

ACTOR Bandagespower : PowerupGIVER
{
	Inventory.MaxAmount 10
	Scale 0.4
	+INVENTORY.AUTOACTIVATE
	+INVENTORY.ADDITIVETIME
	powerup.type Regeneration
	powerup.duration -10
	powerup.strength 1
	Tag "Bandages"
	States
	{
		Spawn:
		BNDG B -1
		Stop
	}
}	

