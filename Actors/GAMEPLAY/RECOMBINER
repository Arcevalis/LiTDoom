	////////////	
	// DISMANTLING
	///////////
	
DismantleStart:
    RDM1 A 0 A_radiusgive("removeingredient",100, RGF_NOSIGHT | RGF_ITEMS , 1, "droppedcell", "none", 0, 2)
	"####" "#" 3
DismantleStartb:
	RDM1 A 1 A_checkproximity("Dismantle_Sentry", "sentryboxinventory",30, 1)
	R1BA A 0 A_checkproximity("Dismantle_Beacon", "Beacon",30, 1)
	R1BA A 0 A_checkproximity("Dismantle_Teleporter", "SentryBotTeleportBeacon",30, 1)
	R1BA A 0 A_checkproximity("Dismantle_NVG", "infrared2",30, 1)
	R1BA A 0 A_checkproximity("Dismantle_Nuke", "nuke",30, 1)
	R1BA A 0 A_checkproximity("Dismantle_FusionCore", "bfgcoreammo",30, 1)
	R1BA A 0 A_checkproximity("Dismantle_Container", "artifact_container",30, 1)
	R1BA A 0 A_checkproximity("Dismantle_RepairDevice", "repairdevice",30, 1)
	R1BA A 0 A_checkproximity("Dismantle_Radar", "radar",30, 1)
	R1BA A 0 A_checkproximity("Dismantle_LifeModule", "lifemodule",30, 1)
	R1BA A 0 A_checkproximity("Dismantle_ForceField", "ForceFieldG",30, 1)
	R1BA A 0 A_checkproximity("Dismantle_ETCModule", "ETCRifleModule",30, 1)
	loop

/// SENTRY	
 Dismantle_Sentry:
    RDM2 A 0 A_radiusgive("removeingredient",100, RGF_NOSIGHT | RGF_ITEMS , 1, "sentryboxinventory", "none", 0, 1)
	"####" "#" 3
 Dismantle_Sentryb:
	RDM2 A 1 A_checkproximity("Dismantle_Sentry_Ready", "droppedcell",30, 1)
	loop
 Dismantle_Sentry_Ready:
		TNT1 A 0 A_radiusgive("removeingredient",100, RGF_NOSIGHT | RGF_ITEMS , 1, "droppedcell", "none", 0, 2)
		RDM3 A 5 
 Dismantle_Sentry_Readyb:
		RDM3 A 5
		TNT1 A 0 A_checkproximity("Dismantle_Sentry_Go", "droppedcell",30, 1)
		loop
Dismantle_Sentry_Go:	
		R3CL A 0 A_playsound("Claymore/Trigger",0,0.5)
		RDM3 AAAA 1 A_radiusgive("removeingredient",100, RGF_NOSIGHT | RGF_ITEMS , 1, "droppedcell", "none", 0, 2)
		R3BK A 0 A_playsound("CRAFT",7)
		RDM3 AAAAAAAAAA 1 A_radiusgive("removeingredient",100, RGF_NOSIGHT | RGF_ITEMS , 1, "droppedcell", "none", 0, 2)
		RDM3 B 5 BRIGHT
		R3CL A 4		
		RECR AAAAAAAAAAAAAAAAAAAAAAAAABCDEFGHILMNOBCDEFGHILMNOBCDEFGHILMNOBCDEFGHILMNOBCDEFGHILMNOBCDEFGHILMNO 1 A_Lookex(LOF_NOJUMP,0,250,250,360,"Dismantle_Sentry_Yield")
Dismantle_Sentry_Yield:
		RECR O 1 A_Facetarget
		TNT1 AAA 0 A_Spawnitemex("metalcrafted",2,15, 30,0,0,0,0)
		TNT1 A 0 A_Spawnitemex("electroniccrafted",2,0, 30,0,0,0,0)
		R3SP A 0 A_stopsound(7)
		R3SP A 0 A_playsound("DSPISTOL",7,1.0,0)
		RECS NOPQ 1
		GOTO CHECKDAMAGE

/// BEACON
 Dismantle_Beacon:
    RDM2 A 0 A_radiusgive("removeingredient",100, RGF_NOSIGHT | RGF_ITEMS , 1, "beacon", "none", 0, 1)
	"####" "#" 3
 Dismantle_Beaconb:
