Actor Recombinerpickup : CustomInventory 30016
{
  +COUNTITEM
  +INVENTORY.INVBAR
  +CANNOTPUSH

  Inventory.InterHubAmount 1
  Inventory.MaxAmount 1
  Inventory.Icon "RECIA0"
  Inventory.PickupMessage "$GOTRECOMBINER"
  Inventory.PickupSOUND "RECOPKUP"
  TAG "PORTABLE RECOMBINER"
  Scale 0.3
  States
  {
  Spawn:
    RECP B -1
    Stop
  Use:
    TNT1 A 1 A_SpawnItemEx ("RecombinerStart", 56, 0, 8, 0, 0,0, 0, SXF_SETTARGET)
    Stop
	}
}

Actor RecombinerDamaged : Recombinerpickup
{
  Inventory.Icon "RECDA0"
  States
  {
  Use:
    TNT1 A 1 A_SpawnItemEx ("RecombinerStartDamaged", 56, 0, 8, 0, 0,0, 0)
    Stop
	}
}


