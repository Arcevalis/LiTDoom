ACTOR ShotgunSpawnerDroped: ShotgunMixed 30054
{
+ROLLSPRITE
+ROLLCENTER
    States
    {
	 Spawn:
		TNT1 A 0
		SGNW B 2
		TNT1 A 0 A_setroll(roll+5)
		TNT1 A 0 A_jump(160,"Break")
	STAY:
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		SGNW B 2
		Loop
		
	Break:	
		TNT1 A 0 a_spawnitemex("brokenshotgun",0,0,10,2,2,2,random(0,360))
		stop	
		
	Pickup:
	    TNT1 A 0
		TNT1 A 0 A_JumpifInventory("shotgundurability", 30,1)
		goto pickuprestore
		TNT1 A 0 A_GiveInventory("Shot_Gun", 1)
		TNT1 A 0 A_GiveInventory("AmmoShell", 1)
		TNT1 A 0 A_takeinventory("JammedShotgun",1)
		Stop
		
	Pickuprestore:
	TNT1 A 0 A_takeinventory("shotgundurability",100)
	TNT1 A 0 A_GiveInventory("shotgundurability",30)
		TNT1 A 0 A_GiveInventory("Shot_Gun", 1)
		TNT1 A 0 A_GiveInventory("AmmoShell", 1)
		TNT1 A 0 A_takeinventory("JammedShotgun",1)
		stop
    }
}

