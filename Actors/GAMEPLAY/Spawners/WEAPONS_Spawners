		goto realpickup
		TNT1 A 0 A_Print("You can only carry one axe")
		Fail
		
	Realpickup:
		TNT1 A 0 A_GiveInventory("BrutalAxe", 1)
		TNT1 A 0 A_TakeInventory("AxeDurability", 30)
		TNT1 A 0 A_GiveInventory("AxeDurability", 30)
		Stop	
