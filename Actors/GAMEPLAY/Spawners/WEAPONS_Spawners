		MGN1 Z 1
		MGN1 Z -1
		Loop
		
	Pickup:
	    TNT1 A 0
		TNT1 A 0 A_GiveInventory("lmgdurability", random(230,300))
		
	Giveammo:
		TNT1 A 0 A_JUMPIFINVENTORY("MACHINEGUN",1,3)
	    TNT1 A 0 A_GiveInventory("LMGAMMO", 30)
		TNT1 A 0 A_GiveInventory("Machinegun", 1)
		Stop
		TNT1 A 0 A_GiveInventory("Clip2", 30)
		TNT1 A 0 A_GiveInventory("Machinegun", 1)
		STOP
    }
}


ACTOR Machinegundropped: MachinegunSpawner
{
    States
    {
		 Spawn:
		MGN1 Z 2
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0 A_jump(127,"Break")
	Stay:
		MGN1 Z 1
		MGN1 Z -1
		Loop
	
	Break:	
		TNT1 A 0 a_spawnitemex("brokenMMG",0,0,10,2,2,2,random(0,360))
		stop		
		
	Pickup:
	    TNT1 A 0
		TNT1 A 0 A_JumpifInventory("lmgdurability", 120,1)
		goto pickuprestore
		TNT1 A 0 
		goto giveammo
		
	Pickuprestore:
	TNT1 A 0 A_takeinventory("lmgdurability",400)
	TNT1 A 0 A_GiveInventory("lmgdurability",120)
		goto giveammo
		
	Giveammo:
		TNT1 A 0 A_JUMPIFINVENTORY("MACHINEGUN",1,3)
		TNT1 A 0 A_GiveInventory("LMGAMMO", 30)
		TNT1 A 0 A_GiveInventory("Machinegun", 1)
		Stop
		TNT1 A 0 A_GiveInventory("Clip2", 30)
		TNT1 A 0 A_GiveInventory("Machinegun", 1)
		STOP
    }
}


