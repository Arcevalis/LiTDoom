 ////////////////////////////////////////////////////////////////////////////////////////	 	 RIFLE		 ////////////////////////////////////////////////////////////////////
ACTOR RifleSpawner: CustomInventory Replaces Pistol 30051
{
 scale 0.40
 radius 24
 height 24
 +THRUACTORS +FORCEXYBILLBOARD
 Inventory.PickupMessage "$GOTRIFLE"
 Inventory.PickupSound "CLIPIN"
  tag "Orthros M360"
 Mass 1
    States
    {

	 Spawn:
		TNT1 A 0
		BADP A 2
		"####" "#" 0  A_ChangeFlag("XFLIP",random(0,1))
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
	Stay:
		BADP A 2
		Loop
	Pickup:
	    TNT1 A 0
	PickupNormal:	
		TNT1 A 0 A_GiveInventory("RIFLEDURABILITY", random(180,250))
		TNT1 A 0 A_GiveInventory("LiTRifle", 1)
		TNT1 A 0 A_GiveInventory("Clip2", 15)
		Stop

    }
}

ACTOR RifleSpawnerDropped: RifleSpawneR
{
 Inventory.PickupMessage ""
    States
    {
	Pickup:
		TNT1 A 0 
	pickupnormal:
		TNT1 A 0 A_JumpifInventory("rifledurability", 90,1)
		goto pickuprestore
		TNT1 A 0 A_GiveInventory("LitRifle", 1)
		TNT1 A 0 A_JUMP(127,"473MM")
		TNT1 A 0 A_LOG("$GOTRIFLE762")
		TNT1 A 0 A_GiveInventory("Clip2", 5)
		Stop

	473MM:
	TNT1 A 0 A_LOG("$GOTRIFLE473")
	TNT1 A 0 A_GiveInventory("MINIGUNAMMO", 8)
	STOP
	

	Pickuprestore:
	TNT1 A 0 A_GiveInventory("rifledurability",90)
		TNT1 A 0 A_GiveInventory("LitRifle", 1)
		TNT1 A 0 A_JUMP(127,"473MM")
		TNT1 A 0 A_LOG("$GOTRIFLE762")
		TNT1 A 0 A_GiveInventory("Clip2", 5)
		stop
    }
}

ACTOR UnfairRifle: RifleSpawner
{
 Inventory.PickupMessage ""
    States
    {
	Pickup:
		TNT1 A 0 
	pickupnormal:
		TNT1 A 0 A_JumpifInventory("rifledurability", 90,1)
		goto pickuprestore
		TNT1 A 0 A_GiveInventory("LitRifle", 1)
		TNT1 A 0 A_JUMP(127,"473MM")
		TNT1 A 0 A_LOG("$GOTRIFLE762")
		TNT1 A 0 A_GiveInventory("Clip2", 3)
		Stop
		
	Pickuprestore:
	TNT1 A 0 A_takeinventory("rifledurability",250)
	TNT1 A 0 A_GiveInventory("rifledurability",90)
		TNT1 A 0 A_GiveInventory("LitRifle", 1)
		TNT1 A 0 A_JUMP(127,"473MM")
		TNT1 A 0 A_LOG("$GOTRIFLE762")
		TNT1 A 0 A_GiveInventory("Clip2", 3)
		stop
		
	473MM:
	TNT1 A 0 A_LOG("$GOTRIFLE473")
	TNT1 A 0 A_GiveInventory("MINIGUNAMMO", 6)
	STOP
    }
}


 ////////////////////////////////////////////////////////////////////////////////////////	 	 PISTOL		 ////////////////////////////////////////////////////////////////////

ACTOR PistolSpawner: CustomInventory 30052
{
 scale 0.22
 radius 24
 height 24
 +THRUACTORS +FORCEXYBILLBOARD
 Inventory.PickupMessage "$GOTPISTOL"
 Inventory.PickupSound "PISTSELE"
 tag "Mediator M99"
 Mass 1
    States
    {
	 Spawn:
		TNT1 A 1
		"####" "#" 0  A_ChangeFlag("XFLIP",random(0,1))
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
	Stay:
		PIST G 1
		PIST G -1
		Stop
	Pickup:
	    TNT1 A 0
	PickupNormal:	
		TNT1 A 0 A_GiveInventory("BrutalPistol", 1)
		TNT1 A 0 A_GiveInventory("Clip1", 5)
		TNT1 A 0 A_GiveInventory("Pistoldurability", random(150,200))
		TNT1 A 0 A_takeinventory("JammedPistol",1)
		Stop
    }
}


ACTOR UnfairPistol: CustomInventory
{
 scale 0.22
 radius 24
 height 24
 +THRUACTORS +FORCEXYBILLBOARD +rollsprite +rollcenter
 Inventory.PickupMessage "$GOTPISTOL"
 Inventory.PickupSound "CLIPIN"
 tag "Mediator 9mm Pistol"
 Mass 1
    States
    {

	 Spawn:
		TNT1 A 1
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0 A_jump(127,"Break")
	Stay:
		"####" "#" 0  A_setroll(roll+27) 
		"####" "#" 0  A_spriteoffset(0.0,8.0)
		PIST G -1
		Stop
		
	Break:	
	TNT1 A 0 a_spawnitemex("brokenpistol",0,0,10,2,2,2,random(0,360))
	stop
		
	Pickup:
	    TNT1 A 0
	PickupNormal:	
		TNT1 A 0 A_JumpifInventory("pistoldurability", 80,1)
		goto pickuprestore
		TNT1 A 0 A_GiveInventory("BrutalPistol", 1)
		TNT1 A 0 A_GiveInventory("Clip1", 3)
		TNT1 A 0 A_takeinventory("JammedPistol",1)
		Stop
	Pickuprestore:
	    TNT1 A 0 A_GiveInventory("Clip1", 3)
		TNT1 A 0 A_takeInventory("Pistoldurability", 100)
		TNT1 A 0 A_GiveInventory("Pistoldurability", 80)
		TNT1 A 0 A_GiveInventory("BrutalPistol", 1)
		TNT1 A 0 A_takeinventory("JammedPistol",1)
		Stop
    }
}


