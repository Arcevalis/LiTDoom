ACTOR Knifenew : KnifeSpawner 30067
{
radius 25
+nogravity
States
{
	 Spawn:
		TNT1 A 0
		TNT1 A 0 
	Stay:
		knfa C 0 A_checkproximity("bench","recombinerstatic",50)
		knfa C 0 A_checkproximity("bench","recombinerstart",50)
		knfa I 1 A_changeflag("NOGRAVITY", 0)
		KNFA I 10 A_SetAngle(random(180,-180))
		KNFA I 1
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		KNFA I -1
		Loop
		
	Bench:
		KNFA I 1
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		KNFA I 1
		CBPK A 0 A_checkproximity("bench","recombinerstatic",50)
		CBPK A 0 A_checkproximity("bench","recombinerstart",50)
		GOTO SPAWN
		
Pickup:
	    TNT1 A 0
		TNT1 A 0 A_GiveInventory("Knife", 1)
		TNT1 A 0 A_GiveInventory("KnivesAmmo", 1)
		TNT1 A 0 A_GiveInventory("Knifedurability", 40)
		Stop
}}

