ACTOR DROPPEDSMG : SMGSPAWNER
{
+vulnerable
health 2
    States
    {
	 Pickup:
	    TNT1 A 0
		TNT1 A 0 A_GiveInventory("BRUTALSMG", 1)
		Stop
		
	death.removeSMG:
	pain.removeSMG:
	TNT1 A 1
	Stop
		
    }
}

ACTOR craftedSMG : SMGSPAWNER
{
SCALE 0.6
+NOGRAVITY
-THRUACTORS
    States
    {
SPAWN:
	SMGZ A 1
	
Stay:
  	MEDM A 0 A_checkproximity("bench","recombinerstatic",50)
	MEDM A 0 A_checkproximity("bench","recombinerstart",50)
	SUPP A 1 A_changeflag("NOGRAVITY", 0)
	SMGZ A -1
	stop
	
BENCH:
	SMGZ A 1
	CBPK A 0 A_checkproximity("bench","recombinerstatic",50)
	CBPK A 0 A_checkproximity("bench","recombinerstart",50)
	GOTO SPAWN
	
	 Pickup:
		TNT1 A 0 A_GiveInventory("BRUTALSMG", 1)
		Stop
    }
}

