dropitem "shotgunbuckshots"
}

ACTOR ShotgunMixed: CustomInventory 
{
yScale 0.51
xscale 0.49
 radius 24
 height 24
 +THRUACTORS +FORCEXYBILLBOARD

 Inventory.PickupMessage "$GOTSHOTGUN"
 Inventory.PickupSound "SGSEL"
  tag "Kampfmann M650"
 Mass 1
    States
    {

	 Spawn:
		SGNW A 0
		"####" "#" 0  A_ChangeFlag("XFLIP",random(0,1))
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0 A_jump(256,"Stay")
	Stay:
		SGNW A 2
		Loop
		
	Pickup:
	    TNT1 A 0
		TNT1 A 0 A_GiveInventory("Shot_Gun", 1)
		TNT1 A 0 A_GiveInventory("AmmoShell", 4)
		TNT1 A 0 A_GiveInventory("AmmoSlug", 2)	
		TNT1 A 0 A_GiveInventory("ShotgunDurability", random(70,100))
		TNT1 A 0 A_takeinventory("JammedShotgun",1)
		Stop
    }
}

