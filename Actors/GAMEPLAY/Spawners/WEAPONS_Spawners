ACTOR RevolverSpawner : CustomInventory 30055
{
 scale 0.4
 radius 24
 height 24
 +THRUACTORS +FORCEXYBILLBOARD +rollsprite +rollcenter
 Inventory.PickupMessage "$GOTREVOLVER"
 Inventory.PickupSound "CYLNSPIN"
   tag "Kriegerhand .500 S&W"
 Mass 1
    States
    {

	 Spawn:
		TNT1 A 0
		"####" "#" 0  A_ChangeFlag("XFLIP",random(0,1))
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		Stay:
		RVIC A 1
		Loop
		
	    Pickup:
	    TNT1 A 0
		TNT1 A 0 A_GiveInventory("Revolver", 1)
		TNT1 A 0 A_GiveInventory("50ammo", 6)
		TNT1 A 0 A_GiveInventory("revolverdurability", random(80,100))
		Stop
    }
}



ACTOR RevolverSpawnerDropped: RevolverSpawner
{
+noclip
    States
    {
	 Spawn:
		TNT1 A 0
		RVIC A 1
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0 A_jump(135,"Break")

	Stay:
		"####" "#" 0  A_setroll(roll+25)
		"####" "#" 0  A_spriteoffset(0.0,10.0)
		RVIC A -1
		Loop
		
	Break:	
		TNT1 A 0 a_spawnitemex("brokenrevolver",0,0,10,2,2,2,random(0,360))
		stop		
	
	Pickup:
	    TNT1 A 0
		TNT1 A 0 A_JumpifInventory("revolverdurability", 40,1)
		goto pickuprestore
		TNT1 A 0 A_GiveInventory("Revolver", 1)
		TNT1 A 0 A_GiveInventory("50ammo", 3)
		Stop
		
	Pickuprestore:
	TNT1 A 0 A_takeinventory("revolverdurability",100)
	TNT1 A 0 A_GiveInventory("revolverdurability",40)
		TNT1 A 0 A_GiveInventory("Revolver", 1)
		TNT1 A 0 A_GiveInventory("50ammo", 3)
		stop
    }
}							 

 ////////////////////////////////////////////////////////////////////////////////////////	 	 SUPER SHOTGUN		 ////////////////////////////////////////////////////////////////////

ACTOR SSGSpawner: CustomInventory 30056
{
 scale 0.35
 radius 24
 height 24
 +THRUACTORS 
 +FORCEXYBILLBOARD
 +movewithsector
 +FLOATbob
 FloatBobStrength 0.1
 Inventory.PickupMessage "$GOTSHOTGUN2"
 Inventory.PickupSound "CLIPINSS"
tag "Akrafena"
 Mass 1
    States
    {

	 Spawn:
		TNT1 A 0
		"####" "#" 0  A_ChangeFlag("XFLIP",random(0,1))
		SOSP A 40 BRIGHT A_AttachLightdef(2,"SMALLREDTORCH")
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		SOSP A 3
		SOSP A 0 A_STOP
		
		Stay:
		TNT1 A 0 a_JUMP(200,3)
		GBPJ AA 0 A_CustomMissile ("redsmokkspawner", 10, 0, random (70, 110), 2, random (0, 360))
		SOSP A 1 BRIGHT A_startsound("SSGWHIS2",12,CHANF_LOOPING, 0.1, 2,0.9,0.05)
		loop
		
	Pickup:
	    TNT1 A 0
		TNT1 A 0 A_STOPSOUND(12)
		TNT1 A 0 A_GiveInventory("SSGCHARGE", 10)
		TNT1 A 0 A_GiveInventory("ssgischarged",1)
		TNT1 A 0 A_GiveInventory("SSG", 1)
		TNT1 A 0 A_GiveInventory("SSGdurability", random(80,130))
		TNT1 A 0 A_GiveInventory("AmmoShell", 2)
		TNT1 A 0 A_GiveInventory("justfoundSSG", 1)
		Stop

    }
}

 ////////////////////////////////////////////////////////////////////////////////////////	 	 ASSAULT SHOTGUN 			 ////////////////////////////////////////////////////////////////////
ACTOR ASGspawner: CustomInventory 30057
{
 scale 0.39
 radius 24
 height 24
 +THRUACTORS +FORCEXYBILLBOARD
 Inventory.PickupMessage "$GOTASG"
 Inventory.PickupSound "ASGUP"
 tag "Skallkvern-12"
 Mass 1
    States
    {

	 Spawn:
		TNT1 A 0
	Stay:
		A12P A 2
		"####" "#" 0  A_ChangeFlag("XFLIP",random(0,1))
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		A12P A -1
		Stop	
		
	Pickup:
	TNT1 A 0
	TNT1 A 0 A_GiveInventory("ASGdurability", random(80,100))
	
	giveammo:
	TNT1 A 0 A_JumpifInventory("AssaultShotgun", 1,3)
	TNT1 A 0 A_GiveInventory("ASSAULTSHOTGUNammo", 10)
	TNT1 A 0 A_GiveInventory("ASSAULTSHOTGUN", 1)
	stop
	TNT1 A 0 A_GiveInventory("ammoshell", 10)
	TNT1 A 0 A_GiveInventory("ASSAULTSHOTGUN", 1)
	stop
    }
}


ACTOR ASGDROPPED: ASGspawner
{
    States
    {
	Spawn:
		TNT1 A 0
	Stay:
		A12P A 10
		TNT1 A 0 A_ChangeFlag("THRUACTORS", 0)
		TNT1 A 0 A_jump(127,"Break")
		A12P A -1

	Break:	
		TNT1 A 0 a_spawnitemex("brokenASG",0,0,10,2,2,2,random(0,360))
		stop				
		
	Pickup:
	    TNT1 A 0
		TNT1 A 0 A_JumpifInventory("ASGdurability", 50,1)
		goto pickuprestore
		TNT1 A 0
		goto giveammo
		
	Pickuprestore:
	TNT1 A 0 A_takeinventory("ASGdurability",120)
	TNT1 A 0 A_GiveInventory("ASGdurability",50)
	goto giveammo
	
	giveammo:
	TNT1 A 0 A_JumpifInventory("AssaultShotgun", 1,3)
	TNT1 A 0 A_GiveInventory("ASSAULTSHOTGUNammo", 10)
	TNT1 A 0 A_GiveInventory("ASSAULTSHOTGUN", 1)
	stop
	TNT1 A 0 A_GiveInventory("ammoshell", 10)
	TNT1 A 0 A_GiveInventory("ASSAULTSHOTGUN", 1)
	stop
    }
}


