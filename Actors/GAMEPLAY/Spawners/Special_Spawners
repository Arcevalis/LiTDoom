Actor MapCombiner : LiT_ReplacementPickup Replaces Allmap
{
    States
    {
	Spawn:
		TNT1 A 1
		TNT1 A 0 A_Jump(127, "Recombiner")  
		
	Automap:
		TNT1 A 0 A_SpawnItemEx ("Allmap2",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION  ,0)
		GOTO LOOPLET
	
	 Recombiner:
		TNT1 A 0 A_SpawnItemEx ("RecombinerPickup",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION  ,0)
		GOTO LOOPLET
    }
}
Actor Allmap2 : Allmap {inventory.pickupsound "DTAMAP" tag "computer area map"}



