//BD Mutators Friendly Spawner
Actor SupportSpawner : LiT_ReplacementPickup Replaces BlurSphere
{
    States
    {
	Spawn:
		TNT1 A 1
		TNT1 A 0 A_Jump(5, "bfgcore") 
		TNT1 A 0 A_Jump(15, "beacon") 
		TNT1 A 0 A_Jump(45, "sentry") 
		TNT1 A 0 A_Jump(100, "corpse") 
		TNT1 A 0 A_Jump(60, "Jar") 
	
	BFGcore:
		TNT1 A 0 A_SpawnItemEx ("bfgcoreammo",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION  ,0)
		GOTO LOOPLET
	
	Spawn1:
		TNT1 A 0 A_SpawnItemEx ("CapturedMarine",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION  ,0)
		GOTO LOOPLET
	
	 sentry:
		TNT1 A 0 A_SpawnItemEx ("SentryBox",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION  ,0)
		GOTO LOOPLET

	Jar:
	TNT1 A 0 A_SpawnItemEx ("Artifact_Container",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION  ,0)
	GOTO LOOPLET
	
	 beacon:
		TNT1 A 0 A_SpawnItemEx ("Beacon",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION  ,0)
		GOTO LOOPLET
	
	corpse:	
		TNT1 A 0 A_JUMP(127,2)
		TNT1 A 0 A_SpawnItemEx("deadmarinedrop",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION  ,0)
		GOTO LOOPLET
		TNT1 A 0 A_SpawnItemEx("deadscientistdrop",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION  ,0)
		GOTO LOOPLET
   }
}

	

