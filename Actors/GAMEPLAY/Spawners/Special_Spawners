//Random Spawners for new Powerups
Actor BD_Mut_OverpoweredSpawner : LiT_ReplacementPickup Replaces InvulnerabilitySphere
{
    States
    {
	Spawn:
		TNT1 A 1
		TNT1 A 0 A_Jump(127, "Time","Relic") 	

		TNT1 A 0 A_SpawnItemEx ("InvulnerabilitySphere_",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION  ,0)
		GOTO LOOPLET
	
	 Time:
		TNT1 A 0 A_SpawnItemEx ("TimeSphere",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION  ,0)
		GOTO LOOPLET
	
	 Relic:
		TNT1 A 0 A_SpawnItemEx ("Allsphere",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION  ,0)
		GOTO LOOPLET
    }
}

