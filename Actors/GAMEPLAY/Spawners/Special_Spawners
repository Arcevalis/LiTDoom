Actor DeadMaleScientist
{
  Radius 32
  Height 16
  States
  {
  Spawn:
    TNT1 A 0
	TNT1 A 0 A_SpawnItem("RatSpawner")
	PLAY M 0 A_SpawnItemEx ("ActiveFlarewaiting",0,0,20,random(3,7),0,0,random(0,360),0,100)
	TNT1 AAAAA 0 A_CustomMissile ("SuperWallRedBlood", 10, 0, random (0, 360), 2, random (5, 40))
	TNT1 AAAAAA 0 A_SpawnItemEx("Brutal_BloodSpot",random(64, -64),random(64, -64))
	TNT1 A 0 A_SpawnItem("MediumBloodSpot")
    MSCI N 1

	Rest:
	MSCI N -1
	loop
  }
}

Actor DeadFemaleScientist
{
  Radius 32
  Height 16
  States
  {
  Spawn:
	TNT1 A 0
	TNT1 A 0 A_SpawnItem("RatSpawner")
	PLAY M 0 A_SpawnItemEx ("ActiveFlarewaiting",0,0,20,random(3,7),0,0,random(0,360),0,100)
	TNT1 AAAAA 0 A_CustomMissile ("SuperWallRedBlood", 10, 0, random (0, 360), 2, random (5, 40))
	TNT1 AAAAAA 0 A_SpawnItemEx("Brutal_BloodSpot",random(64, -64),random(64, -64))
	TNT1 A 0 A_SpawnItem("MediumBloodSpot")
    FSCI N 1

	Rest:
	FSCI N -1
    Loop
  }
}	

Actor deadscientistdrop : CustomInventory
{
    States
    {
	Spawn:
		TNT1 A 1
		TNT1 A 0 A_Jump(127, "male") // spawn a male scientist instead
		
