actor CHEMSPAWNER : CustomInventory
{
	+Inventory.QUIET
    States
    {

	 Spawn:
		TNT1 A 0
		TNT1 A 0 A_jump(255, "ACID", "NEUTRAL", "BASIC")
		loop
		
		ACID:
		TNT1 A 1 A_SpawnItemEx ("AcidVial",0,0,0,(random(0,2)),(random(0,2)),2,(random(-180,180)),SXF_NOCHECKPOSITION)
		Stop
		
		NEUTRAL:
		TNT1 A 1 A_SpawnItemEx ("NeutralVial",0,0,0,(random(0,2)),(random(0,2)),2,(random(-180,180)),SXF_NOCHECKPOSITION)
		Stop
		
		BASIC:
		TNT1 A 1 A_SpawnItemEx ("BasicVial",0,0,0,(random(0,2)),(random(0,2)),2,(random(-180,180)),SXF_NOCHECKPOSITION)
		Stop
}}


actor DRUGSPAWNER : CustomInventory
{
	+Inventory.QUIET
    States
    {

	 Spawn:
		TNT1 A 0
		TNT1 A 0 A_jump(255, "healstim", "healstim2", "ironstim", "rockskin", "staminastim", "berserkstim", "rampagestim","chaosstim", "pheromones", "predatorstim", "clairvoyance", "hellfireammo")
		loop
		
		healstim:
		TNT1 A 1 A_SpawnItemEx ("healstim",0,0,0,(random(0,2)),(random(0,2)),2,(random(-180,180)),SXF_NOCHECKPOSITION)
		Stop
		
