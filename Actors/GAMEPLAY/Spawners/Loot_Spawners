///MILITARY
actor MilitarybagOpen
{
+FLOORCLIP
+thruactors
  States
  {

  Spawn:
	ABAG A 0
	ABAG A 1 A_CHECKFLOOR(1)
	LOOP
	ABAG A 0 a_stop
	ABAG AAAAAAAA 0 A_SPAWNITEMEX("SMOKESPAWNER",0,0,0,1,1,1,RANDOM(0,360),SXF_NOCHECKPOSITION)
	ABAG A 20 a_startsound("BAGDROP", 888,0,0.7)
	
	TNT1 A 0 a_startsound("ZIPPER2", 889)
	ABAG A 2 A_SETSCALE(1,0.8)
	ABAG A 2 A_SETSCALE(1,0.6)
	ABAG A 2 A_SETSCALE(1,0.4)
	ABAG A 2 A_SETSCALE(1,0.2)
	TNT1 A 0 A_jump(200, "Ammo", "Ammor")
	goto Weapon

  Ammo:
	ABAG A 1 A_SpawnItemEx("AmmolootSmall",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	ABAG A 1 A_SpawnItemEx("AmmolootSmall",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	ABAG AAAAA 0 A_SpawnItemEx("ARMORSHARD",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,120)
	goto rest
  AMMOR:
	ABAG A 1 A_SpawnItemEx("AmmolootSmall",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	TNT1 A 0 A_jump(255, "BigArmor", "StandardArmor", "MehArmor")	
BigArmor:
	ABAG A 1 A_SpawnItemEx("rareheavyarmor",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,120)
	goto rest
StandardArmor:
	ABAG A 1 A_SpawnItemEx("lightarmorspawner",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,120)
	goto rest
MehArmor:
	ABAG A 1 A_SpawnItemEx("unfairlightarmor",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,120)
	goto rest
	
  WEAPON:
	ABAG A 1 A_SpawnItemEx("WeaponlootMedium",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	ABAG A 1 A_SpawnItemEx("AmmolootSmall",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	ABAG A 1 A_SpawnItemEx("ArmorUniform",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,120)
	ABAG A 1 A_SpawnItemEx("ArmorHelmet",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,120)
	ABAG A 1 A_SpawnItemEx("ArmorBoots",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,120)
	goto rest
	
  Rest:
  ABAG A 1
   loop
}}

actor SMALLMilitarybagOpen
{
  Scale 0.75
  
  +FLOORCLIP
  +thruactors
  States
  {
    Spawn:
	ABAG A 0
	ABAG A 1 A_CHECKFLOOR(1)
	LOOP
	ABAG A 0 a_stop
	ABAG AAAAAAAA 0 A_SPAWNITEMEX("SMOKESPAWNER",0,0,0,1,1,1,RANDOM(0,360),SXF_NOCHECKPOSITION)
	ABAG A 20 a_startsound("BAGDROP", 888,0,0.4)
	
	TNT1 A 0 a_startsound("ZIPPER2", 889)
	ABAG A 2 A_SETSCALE(0.75,0.7)
	ABAG A 2 A_SETSCALE(0.75,0.6)
	ABAG A 2 A_SETSCALE(0.75,0.4)
	ABAG A 2 A_SETSCALE(0.75,0.2)
	TNT1 A 0 A_jump(200, "Ammo", "Ammor")
	goto Weapon
    Ammo:
	ABAG AAA 1 A_SpawnItemEx("AmmolootSmall",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	ABAG A 0 A_SpawnItemEx("bfgcoreammo",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,250)
	ABAG AAA 0 A_SpawnItemEx("ARMORSHARD",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,120)
	goto rest
  AMMOR:
	ABAG A 1 A_SpawnItemEx("ArmorUniform",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,120)
	ABAG A 1 A_SpawnItemEx("ArmorBOOTS",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,150)
	ABAG AAAAA 0 A_SpawnItemEx("ARMORSHARD",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	TNT1 A 0 A_jump(127, "ammo")	
	goto rest	
  WEAPON:
	ABAG A 1 A_SpawnItemEx("Weaponlootsmall",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	ABAG A 1 A_SpawnItemEx("ArmorUniform",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,120)
	goto rest
	
  Rest:
   ABAG A 1
   loop
}}

