Actor MILDUFFLEBAG : CustomInventory
{Inventory.PickupSound "ARMRPK"
Inventory.PickupMessage "$GOTMILBAG"
INVENTORY.MAXAMOUNT 1
Inventory.Icon "ABAGC0"
tag "military duffle bag"
+invbar
+NOCLIP
+CANNOTPUSH
+FORCEXYBILLBOARD
 States
 {
  Spawn:
	ABAG A 1
	Stay:
	ABAG A 1 A_CHANGEFLAG("NOCLIP",0)
	ABAG A -1
	Stop
  
 Use:
	CBPK A 0 a_spawnitemex("militarybagopen",0,0,7,5,0,5)
	Stop
  }}

