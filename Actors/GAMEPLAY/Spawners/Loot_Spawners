	LOOP
	MBAG A 0 a_stop
	MBAG AAAAAAAA 0 A_SPAWNITEMEX("SMOKESPAWNER",0,0,0,1,1,1,RANDOM(0,360),SXF_NOCHECKPOSITION)
	MBAG A 20 a_startsound("BAGDROP", 888,0,0.4)
	
	TNT1 A 0 a_startsound("ZIPPER2", 889)
	MBAG A 2 A_SETSCALE(0.75,0.7)
	MBAG A 2 A_SETSCALE(0.75,0.6)
	MBAG A 2 A_SETSCALE(0.75,0.4)
	MBAG A 2 A_SETSCALE(0.75,0.2)
	
	MBAG AAAAA 1 A_SpawnItemEx("HEALTHPLUS",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	TNT1 A 0 A_jump(50, 3)
	MBAG AA 1 A_SpawnItemEx("BANDAGES",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	MBAG AAA 1 A_SpawnItemEx("HEALTHPLUS",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	TNT1 A 0 A_jump(10, "STIM")
	goto rest
	MBAG A 1 A_SpawnItemEx("healstim",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	MBAG A 1 A_SpawnItemEx("HEALTHPLUS",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	TNT1 A 0 A_jump(10, "STIM")
	goto rest
	
  STIM:
  MBAG A 0 A_SpawnItemEx("DRUGSPAWNER",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
  goto rest
	
	
  Rest:
  MBAG A 1
   loop
}}

