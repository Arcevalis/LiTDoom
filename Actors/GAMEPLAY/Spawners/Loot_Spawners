actor SMALLMATSbagOpen :  MEDIbagOpen
{
  Scale 0.75
  +FLOORCLIP
  +thruactors
  States
  {
    Spawn:
	CBAG A 0
	CBAG A 1 A_CHECKFLOOR(1)
	LOOP
	CBAG A 0 a_stop
	CBAG AAAAAAAA 0 A_SPAWNITEMEX("SMOKESPAWNER",0,0,0,1,1,1,RANDOM(0,360),SXF_NOCHECKPOSITION)
	CBAG A 20 a_startsound("BAGDROP", 888,0,0.4)
	
	TNT1 A 0 a_startsound("ZIPPER2", 889)
	CBAG A 2 A_SETSCALE(0.75,0.7)
	CBAG A 2 A_SETSCALE(0.75,0.6)
	CBAG A 2 A_SETSCALE(0.75,0.4)
	CBAG A 2 A_SETSCALE(0.75,0.2)
	
	CBAG A 0 A_SpawnItemEx("EMPTYVIAL",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	CBAG A 0 A_SpawnItemEx("FABRIC",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	CBAG A 0 A_SpawnItemEx("ammocell",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,180)
	CBAG A 0 A_SpawnItemEx("SCRAPMETAL",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION, 50)
	CBAG A 1 A_SpawnItemEx("ELECTRONICPARTS",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,200)
	CBAG A 1 A_SpawnItemEx("GUNPOWDER",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,150)
	CBAG A 1 A_SpawnItemEx("CHEMSPAWNER",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	CBAG A 1 A_SpawnItemEx("DEMONINGREDIENT",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,200)
	goto rest

  Rest:
  CBAG A 1
   loop
}}

actor BIGMATSbagOpen
{
  Scale 1.5
  +FLOORCLIP
  +thruactors
  States
  {
    Spawn:
	CBAG A 0
	CBAG A 1 A_CHECKFLOOR(1)
	LOOP
	CBAG A 0 a_stop
	CBAG AAAAAAAA 0 A_SPAWNITEMEX("SMOKESPAWNER",0,0,0,1,1,1,RANDOM(0,360),SXF_NOCHECKPOSITION)
	CBAG A 20 a_startsound("BAGDROP", 888)
	
	TNT1 A 0 a_startsound("ZIPPER2", 889)
	CBAG A 2 A_SETSCALE(1.5,1.0)
	CBAG A 2 A_SETSCALE(1.5,0.8)
	CBAG A 2 A_SETSCALE(1.5,0.6)
	CBAG A 2 A_SETSCALE(1.5,0.3)

	CBAG AA 0 A_SpawnItemEx("EMPTYVIAL",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	CBAG AA 0 A_SpawnItemEx("FABRIC",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	CBAG A 0 A_SpawnItemEx("ammocell",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	CBAG AA 0 A_SpawnItemEx("SCRAPMETAL",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	CBAG A 1 A_SpawnItemEx("ELECTRONICPARTS",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	CBAG AA 1 A_SpawnItemEx("GUNPOWDER",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,100)
	CBAG AA 1 A_SpawnItemEx("CHEMSPAWNER",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,40)
	CBAG A 1 A_SpawnItemEx("DEMONINGREDIENT",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	CBAG A 1 A_SpawnItemEx("RARESPAWNER",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,100)
	goto rest

  Rest:
  CBAG A 1
   loop
}}


