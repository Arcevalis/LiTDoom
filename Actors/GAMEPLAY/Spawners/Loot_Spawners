	ABAG AAAAAAAA 0 A_SPAWNITEMEX("SMOKESPAWNER",0,0,0,1,1,1,RANDOM(0,360),SXF_NOCHECKPOSITION)
	ABAG A 20 a_startsound("BAGDROP", 888,0)
	
	TNT1 A 0 a_startsound("ZIPPER2", 889)
	ABAG A 2 A_SETSCALE(1.5,1.0)
	ABAG A 2 A_SETSCALE(1.5,0.8)
	ABAG A 2 A_SETSCALE(1.5,0.6)
	ABAG A 2 A_SETSCALE(1.5,0.3)
	TNT1 A 0 A_jump(200, "Ammo", "Ammor")
	goto Weapon
 Ammo:
	ABAG AAAAAAAA 0 A_SpawnItemEx("ARMORSHARD",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,120)
	ABAG A 0 A_SpawnItemEx("bandolier",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	ABAG A 0 A_SpawnItemEx("bfgcoreammo",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,240)
	ABAG A 1 A_SpawnItemEx("POORAmmo",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	ABAG AA 1 A_SpawnItemEx("AmmolootSmall",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	goto rest
 AMMOR:
	ABAG AA 1 A_SpawnItemEx("AmmolootSmall",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,50)
	ABAG A 1 A_SpawnItemEx("rareheavyarmor",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	goto rest

  WEAPON:
	ABAG A 1 A_SpawnItemEx("Weaponloot",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	ABAG A 1 A_SpawnItemEx("ArmorUniform",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,120)
	ABAG A 1 A_SpawnItemEx("ArmorHELMET",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,150)
	ABAG A 1 A_SpawnItemEx("ArmorBOOTS",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,150)
	ABAG A 1 A_SpawnItemEx("AmmolootSmall",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION,150)
	goto rest
Rest:
    ABAG A 1
   loop
}}

///MEDICAL
actor MEDIbagOpen
{
+FLOORCLIP
+thruactors
  States
  {

  Spawn:
	MBAG A 0
	MBAG A 1 A_CHECKFLOOR(1)
	LOOP
	MBAG A 0 a_stop
	MBAG AAAAAAAA 0 A_SPAWNITEMEX("SMOKESPAWNER",0,0,0,1,1,1,RANDOM(0,360),SXF_NOCHECKPOSITION)
	MBAG A 20 a_startsound("BAGDROP", 888,0,0.7)
	
	TNT1 A 0 a_startsound("ZIPPER2", 889)
	MBAG A 2 A_SETSCALE(1,0.8)
	MBAG A 2 A_SETSCALE(1,0.6)
	MBAG A 2 A_SETSCALE(1,0.4)
	MBAG A 2 A_SETSCALE(1,0.2)

	MBAG AAA 1 A_SpawnItemEx("HEALTHPLUS",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	MBAG AAA 1 A_SpawnItemEx("BANDAGES",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	MBAG A 1 A_SpawnItemEx("healstim",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
	TNT1 A 0 A_jump(30, "STIM")
	goto rest
	
  STIM:
  MBAG A 0 A_SpawnItemEx("DRUGSPAWNER",0,0,0,1,1,3,random(0,360),SXF_NOCHECKPOSITION)
  goto rest
	
  Rest:
  MBAG A 1
   loop
}}

actor SMALLMEDIbagOpen :  MEDIbagOpen
{
  Scale 0.75
  +FLOORCLIP
  +thruactors
  States
