actor PoorAmmo : CustomInventory
{
	+Inventory.QUIET
    States
    {

	 Spawn:
		TNT1 A 0
		TNT1 A 0 A_jump(255, "Shells", "Bullets", "Explosives", "CellGas", "Bandolier")
		loop
		
	Vanilla:	
	Shells:
		TNT1 A 0
		TNT1 AA 1 A_SpawnItemEx("ShellSPAWNER",0,0,0,velx,vely,velz,angle,SXF_NOCHECKPOSITION,205)
		TNT1 A 0 A_jump(30, "Spawn")
		TNT1 A 1
		stop
	
	 Bullets:
		TNT1 A 0
		TNT1 A 1 A_SpawnItemEx("Clip2",0,0,0,velx,vely,velz,angle,SXF_NOCHECKPOSITION,210)
		TNT1 A 1 A_SpawnItemEx("Clip1",0,0,0,velx,vely,velz,angle,SXF_NOCHECKPOSITION,210)
		TNT1 A 1 A_SpawnItemEx("Clip1",0,0,0,velx,vely,velz,angle,SXF_NOCHECKPOSITION,210)
		TNT1 A 0 A_jump(30, "Spawn")
		TNT1 A 1
		stop
		
	Explosives:
		TNT1 A 0
		TNT1 A 1 A_SpawnItemEx("Ammorocket",0,0,0,velx,vely,velz,angle,SXF_NOCHECKPOSITION,185)
		TNT1 A 1 A_SpawnItemEx("grenadeammo",0,0,0,velx,vely,velz,angle,SXF_NOCHECKPOSITION,130)
		TNT1 A 1 A_SpawnItemEx("PROXBOMB",0,0,0,velx,vely,velz,angle,SXF_NOCHECKPOSITION,185)
		TNT1 A 0 A_jump(30,"Spawn")
		TNT1 A 1
		stop	
		
	Cellgas:
		TNT1 A 0
		TNT1 A 1 A_SpawnItemEx("Ammocell",0,0,0,velx,vely,velz,angle,SXF_NOCHECKPOSITION,170)
		TNT1 A 1 A_SpawnItemEx("GASAMMO",0,0,0,velx,vely,velz,angle,SXF_NOCHECKPOSITION,190)
		TNT1 A 1 A_SpawnItemEx("ammocell",0,0,0,velx,vely,velz,angle,SXF_NOCHECKPOSITION,170)
		TNT1 A 0 A_jump(30, "Spawn")
		TNT1 A 1
		stop	
		
	Bandolier:
		TNT1 A 0
		TNT1 A 1 A_SpawnItemEx ("Bandolier",0,0,0,velx,vely,velz,angle,SXF_NOCHECKPOSITION,170)
		TNT1 A 0 A_jump(30, "Spawn")
		TNT1 A 1
		stop
}}

