      RCSE ABCDEFGHABCDEFGHABCDEFGHABCDEFGHABCDEFGH 4
      Stop
   Death:
      RCSE G 600
      Loop
   }
}

ACTOR emptyammobox : emptyrocketchamber
{
	scale 0.55
	 gravity 1.8
	 BounceFactor 0.2
	BounceCount 3
	WallBounceFactor 0.2
	+ROLLSPRITE
	+ROLLCENTER
	+bounceonfloors
	+bounceonwalls
		BounceSound "mbounce2"
	DeathSound "bigmag"
scale 0.6
	states
	{
	 Spawn:
      TNT1 A 0
	Flying:
        LMGB O 1 a_setroll(roll+15)
		loop
     Death:
		TNT1 A 0 A_JUMP(127, "Death2","Death3","Death4")
		LMGB O 1 a_setroll(15)
        LMGB O -1
        stop
	 Death2:
		LMGB O 1 a_setroll(90)
        LMGB O -1
        stop
	Death3:
		LMGB O 1 a_setroll(180)
        LMGB O -1
        stop
	Death4:
		LMGB O 1 a_setroll(270)
        LMGB O -1
        stop
	  }
}
	

