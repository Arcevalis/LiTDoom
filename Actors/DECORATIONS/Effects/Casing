ACTOR underwaterEmptyBrass: EmptyBrass
{
   Speed 0
   -BOUNCEONWALLS
   Mass 1
   DeathSound "none"
   BounceSound "none"
   States
   {
   Spawn:
      BLUD C 0 ThrustThingZ (0,7,1,0)
      C4S3 ABCD 3 
	  BLUD C 0 ThrustThingZ (0,7,1,0)
	  C4S3 EFGH 3
	  BLUD C 0 ThrustThingZ (0,7,1,0)
      C4S3 ABCD 3 
	  BLUD C 0 ThrustThingZ (0,7,1,0)
	  C4S3 EFGH 3
	   TNT1 A 0 A_SpawnItem("BUBULZ")
      Loop
   }
}

ACTOR ShotgunCasing
{
   Height 2
   Radius 2
   Speed 6
   Scale 0.25
   +DOOMBOUNCE
   -CANBOUNCEWATER
   - NOGRAVITY
   +WINDTHRUST
   +CLIENTSIDEONLY
   +MOVEWITHSECTOR
   +MISSILE
   +NOBLOCKMAP
   -DROPOFF
   +NOTELEPORT
   +FORCEXYBILLBOARD
   +NOTDMATCH
   +GHOST
   //+CANBOUNCEWATER
   //+DONTSPLASH
   Mass 2
   BounceFactor 0.3
   DeathSound "weapons/shell"
   BounceSound "weapons/shell"
   States
   {
    Spawn:
      C4S2 AABBCCDDEEFFGGHHAABBCCDDEEFFGGHH 1 A_CustomMissile("CasingSmoke", 0, 0, random(0, 360), 2, random(85, 90))
      Stop
   Death:
	  TNT1 A 0
	  TNT1 A 0 ACS_NamedExecuteAlways("BDCheckJanitor")//Check Effects
	  TNT1 A 0 ACS_NamedExecuteAlways("BDCheckJanitor2")//Check Effects
	  TNT1 A 0 A_SpawnItem("MicroSmokeColumn")
      LCPJ A 0 A_Jump(256,"Rest1","Rest2","Rest3","Rest4","Rest5","Rest6","Rest7","Rest8")
      Goto Rest1
    Rest1:
      C4S2 I 200
	  TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "Stoping")
      Loop
    Rest2:
      C4S2 J 200
	  TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "Stoping")
      Loop
    Rest3:
      C4S2 K 200
	  TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "Stoping")
      Loop
    Rest4:
      C4S2 L 200
	  TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "Stoping")
      Loop
    Rest5:
      C4S2 M 200
	  TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "Stoping")
      Loop
    Rest6:
      C4S2 I 200
	  TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "Stoping")
      Loop
    Rest7:
      C4S2 J 200
	  TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "Stoping")
      Loop
    Rest8:
      C4S2 K 200
	  TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "Stoping")
   Loop
   
	Stoping:
		TNT1 A 0 
		TNT1 A 0
		Stop
   }
}


ACTOR EmptyClip
{
   Height 2
   Radius 2
   Speed 4
   Scale 0.25
   //+DOOMBOUNCE
   - NOGRAVITY
   +WINDTHRUST
   +CLIENTSIDEONLY
   +MOVEWITHSECTOR
   +MISSILE
   +NOBLOCKMAP
   -DROPOFF
   +NOTELEPORT
   +FORCEXYBILLBOARD
   +NOTDMATCH
   +GHOST
   BounceFactor 0.3
   Bouncecount 3
   Mass 1
   BounceType doom
	BounceSound "mbounce1"
	DeathSound "smolmag"
   +ROLLSPRITE
   +ROLLCENTER
   +FORCEXYBILLBOARD
   States
   {
   Spawn:
      TNT1 A 3 NODELAY A_SetRoll(45)
	SpawnLoop:
      ECLI G 1 A_SetRoll(roll+15)
      loop
   Death:
      ECLI G 200 A_SetRoll(0)
	  TNT1 A 0 A_JumpIf(GetCVAR("bd_infinitecasings")==0,"Stoping")
	  ECLI G -1
      Loop
	Stoping:
		TNT1 A 0
		Stop
   }
}

ACTOR EmptyClipSMG : EMPTYCLIP
{
	yscale 0.18
	xscale 0.27
}

ACTOR EmptyClipRifle : EmptyClip
{
YSCALE 0.06
Xscale 0.07
+ROLLSPRITE
+ROLLCENTER
	BounceSound "mbounce1"
	DeathSound "riflemag"
	States
	{
   Spawn:
      TNT1 A 5
	Fly:
      ECLP A 1 A_SETROLL(ROLL+20)
      loop
   Death:
	  TNT1 A 0 A_PlaySound("MAGTHUD")
	  TNT1 A 0 ACS_NamedExecuteAlways("BDCheckJanitor")//Check Effects
	  TNT1 A 0 ACS_NamedExecuteAlways("BDCheckJanitor2")//Check Effects
      ECLP A 1 A_SETROLL(0)
	  TNT1 A 0 A_JumpIf(GetCVAR("bd_infinitecasings")==0,"Stoping")
	  ECLP A -1
      Loop
	Stoping:
		TNT1 A 0
		Stop
	}
} 

ACTOR EmptyASGDrum : EmptyClip
{
+ROLLSPRITE
+ROLLCENTER
 gravity 1.5
 Scale 0.2
BounceFactor 0.5
BounceCount 4
WallBounceFactor 0.5
+bounceonfloors
+bounceonwalls
	BounceSound "mbounce2"
	DeathSound "medmag"

   States
    {
	Spawn:
		TNT1 A 0
	Flying:
        ASGB X 1 a_setroll(roll+15)
		loop
     Death:
        ASGB X 2 a_setroll(35)
		ASGB X 2 a_setroll(-35)
		ASGB X 2 a_setroll(30)
		ASGB X 2 a_setroll(-30)
		ASGB X 1 a_setroll(20)
		ASGB X 1 a_setroll(-20)
		ASGB X 1 a_setroll(20)
		ASGB X 1 a_setroll(-20)
		ASGB X 1 a_setroll(10)
		ASGB X 1 a_setroll(-10)
		ASGB X 1 a_setroll(10)
		ASGB X 1 a_setroll(-10)
		ASGB X 1 a_setroll(10)
		ASGB X 1 a_setroll(-10)
		ASGB X 1 a_setroll(0)
		ASGB X -1
		stop
	}
}

ACTOR EmptyRocketChamber
{
   Height 2
   Radius 2
   Speed 4
   Scale 0.9
   +DOOMBOUNCE
   - NOGRAVITY
   +WINDTHRUST
   +CLIENTSIDEONLY
   +MOVEWITHSECTOR
   +MISSILE
   +NOBLOCKMAP
