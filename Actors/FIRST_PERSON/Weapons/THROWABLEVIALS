ACTOR ACIDMISTVIAL : BrutalWeapon
{
	Weapon.AmmoUse1 1
	Weapon.AmmoGive1 1
	Weapon.SelectionOrder 99998
	Weapon.AmmoType1 "acidmistammo"
	Obituary "%o was bbq'd by %k's hellfire vial."
    AttackSound "None"
    Inventory.PickupSound "AmmoAPIN"
	Inventory.Pickupmessage "$GOTACIDMIST"
	+undroppable
    +WEAPON.NOAUTOAIM
    +WEAPON.NOALERT
	+WEAPON.NOAUTOFIRE
	//+INVENTORY.UNDROPPABLE
    Scale 0.3
	States
	{
	
	SelectAnimation:
	Ready:
	    TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("acidmistammo", 1, 1)
		Goto Novials
		ACIS ABCD 1 BRIGHT
		Goto Steady
	
	Ready3:
	Steady:
	TNT1 A 0 A_JumpIfInventory("ExecuteDownedEnemy", 1, "DoExecution")
		TNT1 A 0 A_JumpIfInventory("acidmistammo", 1, 1)
		Goto Novials
		ACIS D 0 BRIGHT A_JumpIf(GetPlayerInput(INPUT_BUTTONS) & BT_USE, "Useattackbright")
		ACIS D 0 A_JumpIfInventory("Kicking",1,"DoKick")
		ACIS D 1 BRIGHT A_WeaponReadyLit
		Loop
	
	Novials:
		TNT1 A 1 A_WeaponReady
		RIFG A 0 A_selectweapon("clustergrenades",SWF_SELECTPRIORITY)
		TNT1 A 0 A_JumpIfInventory("weargasmask", 1, "goingtoaction")
		TNT1 A 0 A_JumpIfInventory("removegasmask", 1, "goingtoaction")
		TNT1 A 0 A_JumpIfInventory("battledrugtoken",1,"goingtoaction")
		TNT1 A 0 A_JumpIfInventory("BANDAGETOKEN",1,"goingtoaction")
		TNT1 A 0 A_Jumpifinventory("throwtoken",1, "Quickthrow")
		TNT1 A 0 A_Jumpifinventory("slashtoken",1, "Quickslash")
		TNT1 A 0 A_JumpIfInventory("flaretoken",1,"TossFlare")
		ACII A 0 A_JumpIf(GetPlayerInput(INPUT_BUTTONS) & BT_USE, "Useattackbright")
		TNT1 A 0 A_JumpIf(GetPlayerInput(INPUT_BUTTONS) & BT_USER3, "TOSSGRENADE")
		TNT1 A 0 A_JumpIfInventory("Kicking",1,"DoKick")
        TNT1 A 0 A_JumpIfInventory("Taunting",1,"Taunt")
		TNT1 A 0 A_JumpIfInventory("Salute1", 1, "Salute")
		TNT1 A 0 A_JumpIfInventory("Salute2", 1, "Salute")
		RIFG A 0 A_JumpIfInventory("IsRunning",1,"CheckSprint")
		Goto Ready
	
	Deselect:
		TNT1 A 0
		TNT1 A 0 A_Lower
		Wait
	
	goingtoaction:
		TNT1 A 0 A_PlaySound("weapons/changing")
		ACIS DCBA 1
		TNT1 A 0 A_TakeInventory("StartDualWield", 1)
		TNT1 A 0 A_TakeInventory("readythrow", 1)
		TNT1 A 0 A_JumpIfInventory("weargasmask", 1, "wearanimation")
		TNT1 A 0 A_JumpIfInventory("removegasmask", 1, "removeanimation")
		TNT1 A 0 A_JumpIfInventory("battledrugtoken",1,"drugshot")
		TNT1 A 0 A_JumpIfInventory("BANDAGETOKEN",1,"BANDAGE")
		goto repair
	
	Select: 
		TNT1 A 0  A_Overlay(-10,"FirstPersonLegsStand")
		TNT1 A 0 a_removelight(1)
		TNT1 A 0 A_removelight(2)	
		TNT1 A 0 SetPlayerProperty(0,0,2)
		TNT1 A 0 A_SetRoll(0)
		GRWG A 0 A_Raise
		Loop
	Fire:
	    ACIT ABCD 1	
	Hold:
		ACIT D 1
		TNT1 A 0 A_REFIRE("hold")
		TNT1 A 5
		TNT1 A 0 A_PLaySound ("GRNTOSS")
		ACIT E 2 BRIGHT
		TNT1 A 0 A_FireCustomMissile("AcidMistyeet", random(-2,2), 1, 0, 0, 0, 0)
		VTHR ABCD 1
		VTHR EE 1 
		VTHR FFFF 1
		VTHR F 1 offset(0,34)
		VTHR F 1 offset(0,36)
		VTHR F 1 offset(0,38)
		VTHR F 1 offset(0,40)
		VTHR F 1 offset(0,42)
		VTHR F 1 offset(0,44)
		VTHR F 1 offset(0,46)
		VTHR F 1 offset(0,48)
		VTHR F 1 offset(0,50)
		Goto Ready	
		
 	Spawn:
		ACMI A 1 BRIGHT
		loop
		
	CheckSprint:
		//ACIS D 1 BRIGHT A_WeaponReady(WRF_NOFIRE)
		GRND F 0 A_JumpIfInventory("IsStandingStill", 1, "Ready3")
		GRND F 0 A_JumpIfInventory("IsTacticalClass", 1, "StartSprint")
		Goto Steady
		
	StartSprint:
		ACIS D 1 BRIGHT A_WeaponReady(WRF_NOFIRE)
		TNT1 A 0 A_JumpIfInventory("LARGESTAMINA", 1, 2)
		TNT1 A 0 A_JumpIfInventory("UsedStamina", 70, "Trip")
		TNT1 A 0 A_JumpIfInventory("UsedStamina", 1400, "Trip")
		
	Sprinting:	
		PISP A 0 offset(-9,32) 
		GRND F 0 offset(-9,32) A_JumpIfInventory("IsStandingStill", 1, "Ready3")
		TNT1 A 0 A_JumpIfInventory("LARGESTAMINA", 1, 2)
		TNT1 A 0 A_JumpIfInventory("UsedStamina", 100, "StopSprintTired")
		TNT1 A 0 A_JumpIfInventory("UsedStamina", 2000, "StopSprintTired")
		PLAY A 0 offset(-9,32) ACS_ExecuteAlways(852, 0, 0, 0, 0)//Makes player faster.
		GRND F 0 offset(-9,32) A_JumpIfInventory("Powerstrength2", 1, 2)
		GRND F 0 offset(-9,32) A_GiveInventory("usedstamina", 1)
		GRND F 0 offset(-9,32)
		ACIS D 1 BRIGHT offset(-9,34) A_SetPitch(pitch -0.5)
		ACIS D 1 BRIGHT offset(-6,36) A_SetPitch(pitch -0.5)
		ACIS D 1 BRIGHT offset(-3,38) A_SetPitch(pitch -0.5)
		ACIS D 1 BRIGHT offset(0,38) A_SetPitch(pitch -0.5)
		ACIS D 1 BRIGHT offset(3,36) A_SetPitch(pitch -0.5)
		ACIS D 1 BRIGHT offset(6,34) A_SetPitch(pitch -0.5)
		ACIS D 1 BRIGHT offset(9,32) A_SetPitch(pitch -0.5)
		GRND F 0 offset(9,32) A_WeaponReady(WRF_NOBOB)
		GRND F 0 A_JumpIfInventory("Kicking",1,"DoKick")
        GRND F 0 A_JumpIfInventory("Taunting",1,"Taunt")
		GRND F 0 offset(-9,32) A_GiveInventory("UsedStamina", 3)
		GRND F 0 offset(9,33) A_SpawnItemEx("FootStep", 0, 0, 2, 0, 0, -4)
		ACIS D 1 BRIGHT offset(9,34) A_SetPitch(pitch +0.5)
		ACIS D 1 BRIGHT offset(6,36) A_SetPitch(pitch +0.5)
		ACIS D 1 BRIGHT offset(3,38) A_SetPitch(pitch +0.5)
		ACIS D 1 BRIGHT offset(0,38) A_SetPitch(pitch +0.5)
		ACIS D 1 BRIGHT offset(-3,36) A_SetPitch(pitch +0.5)
		ACIS D 1 BRIGHT offset(-6,34) A_SetPitch(pitch +0.5)
		ACIS D 1 BRIGHT offset(-9,32) A_SetPitch(pitch +0.5)
		GRND F 0 offset(-9,32) A_WeaponReady(WRF_NOBOB)
		GRND F 0 offset(-9,32) A_JumpIfInventory("IsRunning", 1, "Sprinting")
		
		Goto StopSprint
	StopSprintTired:
	//	ACIS D 1 BRIGHT
		PLAY A 0 ACS_ExecuteAlways(853, 0, 0, 0, 0)//Makes player slower.
		GRND F 0 A_PlaySound("Tired", 2)
		ACIS D 1 BRIGHT 
		ACIS D 1 BRIGHT
		ACIS D 1 BRIGHT 
		ACIS D 1 BRIGHT 
		ACIS D 1 BRIGHT 
		Goto STEADY
	StopSprint:
		//ACIS D 1 BRIGHT
		TNT1 A 0 A_JumpIfInventory("LARGESTAMINA", 1, 2)
		TNT1 A 0 A_JumpIfInventory("UsedStamina", 70, "StopSprintTired")
		TNT1 A 0 A_JumpIfInventory("UsedStamina", 1400, "StopSprintTired")
		PLAY A 0 ACS_ExecuteAlways(853, 0, 0, 0, 0)//Makes player slower.
		Goto STEADY
		
	DoKICKVIAL:
		TNT1 A 0 A_JumpIfInventory("LARGESTAMINA", 1, 2)
		TNT1 A 0 A_JumpifInventory("UsedStamina",95,4)
		TNT1 A 0 A_JumpifInventory("UsedStamina",1995,3)
		TNT1 A 0 A_overlay(3,"KICKVIAL")
		TNT1 A 0 a_Jump(256,"Dokick")
		goto Ready3
		TNT1 A 0 A_overlay(3,"KICKVIALtired")
		TNT1 A 0 a_Jump(256,"Dokick")
		goto Ready3	
	KICKVIAL:
		ACIT ABC 1
		ACIT C 12 
		ACIT ABC 1 
		TNT1 A 0 A_JumpIfInventory("isSliding", 1, 1)
		stop
		ACIT A 10
		STOP
	KICKVIALtired:
		ACIT ABC 2
		ACIT C 37 
		ACIT ABC 2
		STOP		
	}
}




ACTOR AcidMistyeet
{
	Radius 4
	Height 4
	Projectile
	Speed 20
    Damage (random (100, 100))
	Damagetype acid
    Gravity 0.7
	Scale 0.4
	Projectile
	+MISSILE
    -NOGRAVITY
    -BLOODSPLATTER
	-EXTREMEDEATH
	+EXPLODEONWATER
	+SKYEXPLODE
	+BOUNCEONFLOORS
	+BOUNCEONWALLS
	+BOUNCEONCEILINGS
	-NOTELEPORT
	+THRUSPECIES
	Species "Marines"
	BounceFactor 0.5
	WallBounceFactor 0.25
    Health 5
    SeeSound "GRNBNCE"
	DeathSound "GLASSEXP"
	Obituary "%o melted"
	+ROLLSPRITE
+ROLLCENTER
+FORCEXYBILLBOARD
	States
	{
	Spawn:
		ACIF ABCDEFGABCDEFGABCDEFG 2 BRIGHT Light("ARMORBONUS") A_SpawnItem("GreenFlareSmall2",0,7)

	Death:
		TNT1 A 0
		TNT1 A 0 A_SpawnItem("GlassExplosion")
		TNT1 A 1 A_SpawnItemex("AcidMistExplosion",0,0,6)
		TNT1 AAA 0 A_SpawnItemex("acidEMBERSBIG",0,0,0,0,0,0,RANDOM(0,360))
		TNT1 AAAAA 0 A_SpawnItemex("acidEMBERSBIG",random(0,35),random(0,35),0,0,0,0,random(0,360))
		TNT1 A 0 A_AlertMonsters(200)
	    HELF J 10 BRIGHT Light("ARMORBONUS") A_SpawnItem("GreenFlareSmall2",0,7)
	
		TNT1 A 0 A_NoBlocking
		EXPG ABCDEFG 1 BRIGHT
		TNT1 AA 0
		Stop
	}
}

ACTOR acidtimer : Inventory { Inventory.MaxAmount 50 }

actor AcidMistExplosion
