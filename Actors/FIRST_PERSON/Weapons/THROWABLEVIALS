ACTOR HELLFIREAmmo : Ammo
{
	Game Doom
	Inventory.PickupMessage "$GOTHELLFIRE"
	Inventory.Amount 1
	Inventory.MaxAmount 6
	Ammo.BackpackAmount 0
	Ammo.BackpackMaxAmount 12
	inventory.pickupsound "GLVIALPK"
	Inventory.Icon "GASHA0"
	Scale 0.3
	damagefactor "Shrapnel", 0.0  damagefactor "Trample", 0.0  damagefactor "headkick", 0.0 damagefactor "use", 0.0 damagefactor "stealth", 0.0
	damagefactor "Head", 0.0	damagefactor "FriendBullet", 0.0    damagefactor "Taunt", 0.0    damagefactor "KillMe", 0.0
	damagefactor "CancelTeleportFog", 0.0	damagefactor "BHFTOnBarrel", 0.0	damagefactor "GibRemoving", 0.0
	damagefactor "SpawnMarine", 0.0	damagefactor "TeleportRemover", 0.0	damagefactor "MinorHead", 0.0 damagefactor "Telefrag", 0.0
	damagefactor "Decaptate", 0.0	damagefactor "MonsterKnocked", 0.0
	DamageFactor "CauseSplash", 0.0 DamageFactor "CauseObjectsToSplashSlime", 0.0 DamageFactor "CauseObjectsToSplashNukage", 0.0
	DamageFactor "CauseObjectsToSplashBlood", 0.0 DamageFactor "CauseObjectsToSplashLava", 0.0
	damagefactor "HelperMarineFatallity", 0.0	damagefactor "Leg", 0.0
	DamageFactor "KillMeBot", 0.0
	DamageFactor "beartrap", 1000.0
	DamageFactor "plasma", 10.0  DamageFactor "EXPLOSIVEIMPACT", 0.1
	tag"Hellfire Vial"
	HEALTH 15
	+SHOOTABLE
	+cannotpush
+actlikebridge
	+ROLLSPRITE
+ROLLCENTER
+FORCEXYBILLBOARD
	States
	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 //A_SetRoll(randompick(0,90,180,270))
		Stay:
		HELF AABBCCBBCCBB 1 BRIGHT A_SpawnItemEX("RedFlareSmall",0,0,-5)
		loop
		
	Death:
	TNT1 A 0 A_SPAWNITEM("HELLFIREEXPLOSION")
	STOP
	}
}

ACTOR HELLFIREVIAL : BrutalWeapon
{
	Weapon.AmmoUse1 1
	Weapon.AmmoGive1 1
	Weapon.SelectionOrder 99998
	Weapon.AmmoType1 "hellfireammo"
	Obituary "%o was bbq'd by %k's hellfire vial."
    AttackSound "None"
    Inventory.PickupSound "AmmoAPIN"
	Inventory.Pickupmessage "$GOTHELLFIRE"
	+UNDROPPABLE
    +WEAPON.NOAUTOAIM
    +WEAPON.NOALERT
	+WEAPON.NOAUTOFIRE
	//+INVENTORY.UNDROPPABLE
    Scale 0.3
	States
	{
	
	SelectAnimation:
	Ready:
	    TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("hellfireammo", 1, 1)
		Goto Novials
		HELS ABC 1 BRIGHT
		Goto Steady
	
	Ready3:
	Steady:
	TNT1 A 0 A_JumpIfInventory("ExecuteDownedEnemy", 1, "DoExecution")
		TNT1 A 0 A_JumpIfInventory("hellfireammo", 1, 1)
		Goto Novials
		HELS D 0 A_JumpIfInventory("Kicking",1,"DoKick")
		HELS D 0 BRIGHT A_JumpIf(GetPlayerInput(INPUT_BUTTONS) & BT_USE, "Useattackbright")
		HELS DEF 3 BRIGHT A_WeaponReadyLit
		Loop
	
	Novials:
		HELS F 0 A_JumpIf(GetPlayerInput(INPUT_BUTTONS) & BT_USE, "Useattackbright")
		TNT1 A 0 A_JumpIfInventory("Kicking",1,"DoKick")
		RIFG A 0 A_selectweapon("acidmistvial",SWF_SELECTPRIORITY)
		TNT1 A 1 A_WeaponReadyLit
		Goto Ready
	
	goingtoaction:
		TNT1 A 0 A_PlaySound("weapons/changing")
		HELS CBA 1
		TNT1 A 0 A_TakeInventory("StartDualWield", 1)
		TNT1 A 0 A_TakeInventory("readythrow", 1)
		TNT1 A 0 A_TakeInventory("throwtoken", 1)
		TNT1 A 0 A_TakeInventory("slashtoken", 1)
		TNT1 A 0 A_TakeInventory("flaretoken", 1)
		PIST F 0 A_TakeInventory("TossGrenade", 1)
		TNT1 A 0 A_JumpIfInventory("weargasmask", 1, "wearanimation")
		TNT1 A 0 A_JumpIfInventory("removegasmask", 1, "removeanimation")
		TNT1 A 0 A_JumpIfInventory("battledrugtoken",1,"drugshot")
		TNT1 A 0 A_JumpIfInventory("BANDAGETOKEN",1,"BANDAGE")
		goto repair
	
	Deselect:
		TNT1 A 0
		TNT1 A 0 A_Lower
		Wait
		
	Select: 
		TNT1 A 0 A_Overlay(-10,"FirstPersonLegsStand")
		TNT1 A 0 a_removelight(1)
		TNT1 A 0 A_removelight(2)	
		TNT1 A 0 SetPlayerProperty(0,0,2)
		TNT1 A 0 A_SetRoll(0)
		GRWG A 0 A_Raise
		Loop
	Fire:
	    HELF LMNO 1 BRIGHT
	Hold:
		HELF O 1 BRIGHT
		TNT1 A 0 A_REFIRE("HOLD")
		TNT1 A 4
		TNT1 A 0 A_PLaySound ("GRNTOSS")
		HELF P 2 BRIGHT
		TNT1 A 0 A_FireCustomMissile("HellfireVialYEET", random(-2,2), 1, 0, 0, 0, 0)
		VTHR ABCD 1
		VTHR EE 1 
		VTHR FFFF 1
		VTHR F 1 offset(0,34)
		VTHR F 1 offset(0,36)
		VTHR F 1 offset(0,38)
		VTHR F 1 offset(0,40)
		VTHR F 1 offset(0,42)
		VTHR F 1 offset(0,44)
		VTHR F 1 offset(0,46)
		VTHR F 1 offset(0,48)
		VTHR F 1 offset(0,50)
		TNT1 A 5
		Goto Ready	
		
 	Spawn:
		HELF AABBCCBBCCBB 1 BRIGHT
		loop
		
	CheckSprint:
		//HELS F 1 BRIGHT A_WeaponReady(WRF_NOFIRE)
		GRND F 0 A_JumpIfInventory("IsStandingStill", 1, "Ready3")
		GRND F 0 A_JumpIfInventory("IsTacticalClass", 1, "StartSprint")
		Goto STEADY
		
	StartSprint:
		HELS D 1 BRIGHT A_WeaponReady(WRF_NOFIRE)
		TNT1 A 0 A_JumpIfInventory("LARGESTAMINA", 1, 2)
		TNT1 A 0 A_JumpIfInventory("UsedStamina", 70, "Trip")
		TNT1 A 0 A_JumpIfInventory("UsedStamina", 1400, "Trip")
		
	Sprinting:	
		PISP A 0 offset(-9,32) 
		GRND F 0 offset(-9,32) A_JumpIfInventory("IsStandingStill", 1, "Ready3")
		TNT1 A 0 A_JumpIfInventory("LARGESTAMINA", 1, 2)
		TNT1 A 0 A_JumpIfInventory("UsedStamina", 100, "StopSprintTired")
		TNT1 A 0 A_JumpIfInventory("UsedStamina", 2000, "StopSprintTired")
		PLAY A 0 offset(-9,32) ACS_ExecuteAlways(852, 0, 0, 0, 0)//Makes player faster.
		GRND F 0 offset(-9,32) A_JumpIfInventory("Powerstrength2", 1, 2)
		GRND F 0 offset(-9,32) A_GiveInventory("usedstamina", 1)
		GRND F 0 offset(-9,32)
		HELS D 1 BRIGHT offset(-9,34) A_SetPitch(pitch -0.5)
		HELS E 1 BRIGHT offset(-6,36) A_SetPitch(pitch -0.5)
		HELS F 1 BRIGHT offset(-3,38) A_SetPitch(pitch -0.5)
		HELS D 1 BRIGHT offset(0,38) A_SetPitch(pitch -0.5)
		HELS E 1 BRIGHT offset(3,36) A_SetPitch(pitch -0.5)
		HELS F 1 BRIGHT offset(6,34) A_SetPitch(pitch -0.5)
		HELS D 1 BRIGHT offset(9,32) A_SetPitch(pitch -0.5)
		GRND F 0 offset(9,32) A_WeaponReady(WRF_NOBOB)
		GRND F 0 A_JumpIfInventory("Kicking",1,"DoKick")
        GRND F 0 A_JumpIfInventory("Taunting",1,"Taunt")
		GRND F 0 offset(-9,32) A_GiveInventory("UsedStamina", 3)
		GRND F 0 offset(9,33) A_SpawnItemEx("FootStep", 0, 0, 2, 0, 0, -4)
		HELS E 1 BRIGHT offset(9,34) A_SetPitch(pitch +0.5)
		HELS F 1  BRIGHT offset(6,36) A_SetPitch(pitch +0.5)
		HELS D 1 BRIGHT offset(3,38) A_SetPitch(pitch +0.5)
		HELS E 1 BRIGHT offset(0,38) A_SetPitch(pitch +0.5)
		HELS F 1 BRIGHT offset(-3,36) A_SetPitch(pitch +0.5)
		HELS D 1 BRIGHT offset(-6,34) A_SetPitch(pitch +0.5)
		HELS E 1 BRIGHT offset(-9,32) A_SetPitch(pitch +0.5)
		GRND F 0 offset(-9,32) A_WeaponReady(WRF_NOBOB)
		GRND F 0 offset(-9,32) A_JumpIfInventory("IsRunning", 1, "Sprinting")
		
		Goto StopSprint
	StopSprintTired:
		//HELS F 1 BRIGHT
		PLAY A 0 ACS_ExecuteAlways(853, 0, 0, 0, 0)//Makes player slower.
		GRND F 0 A_PlaySound("Tired", 2)
		HELS DEFDEF 1 BRIGHT 
		HELS DEFDEF 1 BRIGHT 
		HELS DEFDEF 1 BRIGHT 
		HELS DEFDEF 1 BRIGHT 
		HELS DEFDEF 1 BRIGHT
		Goto STEADY
	StopSprint:
		//HELS F 1 BRIGHT
		TNT1 A 0 A_JumpIfInventory("LARGESTAMINA", 1, 2)
		TNT1 A 0 A_JumpIfInventory("UsedStamina", 70, "StopSprintTired")
		TNT1 A 0 A_JumpIfInventory("UsedStamina", 1400, "StopSprintTired")
		PLAY A 0 ACS_ExecuteAlways(853, 0, 0, 0, 0)//Makes player slower.
		Goto STEADY	
	
	DoKICKVIAL:
		TNT1 A 0 A_JumpIfInventory("isdoomguy", 1, 4)
		TNT1 A 0 A_JumpIfInventory("LARGESTAMINA", 1, 2)
		TNT1 A 0 A_JumpifInventory("UsedStamina",95,4)
		TNT1 A 0 A_JumpifInventory("UsedStamina",1995,3)
		TNT1 A 0 A_overlay(3,"KICKVIAL")
		TNT1 A 0 a_Jump(256,"Dokick")
		goto Ready3
		TNT1 A 0 A_overlay(3,"KICKVIALtired")
		TNT1 A 0 a_Jump(256,"Dokick")
		goto Ready3	
	KICKVIAL:
		HELS LMN 1
		HELS N 12 
		HELS NML 1 
		TNT1 A 0 A_JumpIfInventory("isSliding", 1, 1)
		stop
		HELS L 10
		STOP
	KICKVIALtired:
		HELS LMN 2
		HELS N 37 
		HELS NML 2
		STOP	
		
	}
}


Actor HellfirevialRoll
{
	Radius 4
	Height 4
	Projectile
	Speed 5
    Damage (random (100, 100))
	Damagetype fire
    Gravity 0.7
	Scale 0.4
	Projectile
	+MISSILE
	+EXPLODEONWATER
	+SKYEXPLODE
	+BOUNCEONWALLS
	-NOTELEPORT
	+THRUSPECIES
	Species "Marines"
	BounceFactor 0.5
	WallBounceFactor 0.25
    Health 5
    SeeSound "GRNBNCE"
	DeathSound "none"
	Obituary "%o was BBQ'd by %k 's hellfire vial."
	+ROLLSPRITE
+ROLLCENTER
+FORCEXYBILLBOARD

	States
	{
	Spawn:
		TNT1 A 0 
		HELF ELMELMELMELM 3 BRIGHT
	Roll:
		HELF ELMELMELMELM 3 BRIGHT a_setspeed(speed-0.5)	
	Stay:
		HELF E 1
		loop
		
	Xdeath:
		TNT1 A 0 A_AlertMonsters(200)
	    HELF E 5 A_SpawnItemEX("RedFlareSmall",0,0,-5)
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_SpawnItem("HellfireExplosion")
		EXPL ABCDEFGH 1
		TNT1 AA 0
		stop
		}
}
	

ACTOR Hellfirevialyeet
{
	Radius 4
	Height 4
	Projectile
	Speed 20
    Damage (random (100, 100))
	Damagetype fire
    Gravity 0.7
	Scale 0.4
	Projectile
	+MISSILE
    -NOGRAVITY
    -BLOODSPLATTER
	-EXTREMEDEATH
	+EXPLODEONWATER
	+SKYEXPLODE
	+BOUNCEONFLOORS
	+BOUNCEONWALLS
	+BOUNCEONCEILINGS
	-NOTELEPORT
	+THRUSPECIES
	Species "Marines"
	BounceFactor 0.5
	WallBounceFactor 0.25
    Health 5
    SeeSound "GRNBNCE"
	DeathSound "none"
	Obituary "%o was BBQ'd by %k 's hellfire vial."
	+ROLLSPRITE
+ROLLCENTER
+FORCEXYBILLBOARD

	States
	{
	Spawn:
		HELF DDEEGFGHIIJJKKDDEEGFGHIIJJKK 1 BRIGHT A_SpawnItemEX("RedFlareSmall",0,0,-5)

	Death:
		TNT1 A 0
		TNT1 A 0 A_AlertMonsters(200)
	    HELF JJJJJJJJJJJJJJJ 1 BRIGHT A_SpawnItemEX("RedFlareSmall",0,0,-5)
	
	Explode:
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_SpawnItem("GlassExplosion")
		TNT1 A 0 A_SpawnItem("HellfireExplosion")
		EXPL ABCDEFGH 1
		TNT1 AA 0
		Stop
	}
}


