		TNT1 A 0 A_SpawnItemEx ("acidsmokeslow", 290, 0, 0,1,0,0,10)
		TNT1 A 0 A_SpawnItemEx ("acidsmokeslow", 290, 0, 0,1,0,0,-170)
		
		
		EXPL AAA 0 A_CustomMissile ("AcidExplosion", 0, 0, random (0, 360), 2, random (0, 360))
		TNT1 A 0 A_CustomMissile("acidflare", random (-32, 0), random (-60, 60), random (-32, 32), 2, random (-32, 32))
