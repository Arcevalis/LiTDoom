ACTOR RecombinerSTATIC : RecombinerStart 30017
{
	States
	{
	Spawn:
	TNT1 A 0
	TNT1 A 0 A_CheckCeiling("vial")
	TNT1 A 0 A_GiveInventory("TargetIsAMarine")
	TNT1 A 0 A_GiveInventory("staticrecombine")
	RECS Q 2
	GOTO CHECKDAMAGE
	
	CHECKDAMAGE2:
	RECO A 1 
	TNT1 A 0 a_JUMP(50,"DEATH")
	GOTO EMPTY
	
	Vial:
	TNT1 A 0 A_Spawnitemex("emptyvial",0,0, 0,0,0,0,0)
    TNT1 A 0 A_Spawnitemex("emptyvial",6,6, 0,0,0,0,0)
	TNT1 A 0 A_Spawnitemex("emptyvial",-6,-6, 0,0,0,0,0)
	TNT1 A -1
	Stop
	
	Active:
	RECS A 0 A_playsound("RBUILD",7)
	RECS QPO 2
	RECS NM 4
	RECS A 0 A_playsound("RBUILD",7)
	RECS NM 4
	RECS OPQ 4
	
	Inactive:
	RECS Q 5
	RECS Q 1 A_Spawnitemex("Recombinerstatic",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION,0)
	RECS A 0 A_stopsound(7)
	Stop
	
	}	
}

ACTOR RecombinerRip
{
	Radius 10
	Height 25
	health 10
	DamageFactor "Use", 0.0 DamageFactor "Stealth", 0.0 DamageFactor "headkick", 0.0
	painchance "disarm", 256
	Damagefactor "Disarm", 1000.0
	+shootable
	+DONTTHRUST
	+NEVERTARGET
	+SOLID
	-CANPASS
	+noblood
	States
	{
	Spawn:
	NULL A 0
	RECO WXY 1 BRIGHT A_SpawnItem("RecombinerExplosion")
	RECO Y 1 a_PLAYSOUND("BIGPLSM2")
	TNT1 A 0 a_SPAWNITEM("BarrelExplosionSmokeColumn")
	RECO XYXYXYXYXYXYXYXYXYXYXYXYXYXYXYXYXYXYXYXYXYXYXYXY 2 BRIGHT 
	RECO YZYZYXYZYZYXYZYZYZYYZYZYYZYZYYZYZYYZYZYYZYZYYZYZYYZYZY 4 BRIGHT
	RECO YYYYYYYZZYYYYYYYYZZYYYYYYYZZZZZYYYYYYZZZZZZZZYYYYZZZZZZZYYZ 2
	RECO W 1 a_SPAWNITEM("BarrelExplosionSmokeColumn")
	RECO W -1
	Stop
	
	Pain.disarm:
	Death.disarm:
	RECO W 2 A_Lookex(LOF_NOJUMP,0,250,250,360,"Disassemble")
	Disassemble:
		RECO W 1 A_Facetarget
		TNT1 A 0 A_Spawnitemex("scrapmetal",0,10, 15,0,1,3,random(1,5))
	Death:
		TNT1 A 0 A_Spawnitemex("scrapmetal",0,-10, 15,0,-1,3,random(1,5))
		Stop
}}

ACTOR RecombinerStartDamaged : RecombinerStart
{
Health 150
States
{
Spawn:
		RECS A 10
		RECS A 0 A_playsound("RBUILD")
		RECS ABCDEFGHILMNOPQ 2
		RECS Q 2 BRIGHT
		RECS Q 2
		RECS Q 2 BRIGHT
		RECS Q 1 A_Facetarget
		TNT1 A 0 A_Spawnitemex("Recombinerstatic",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_SETTARGET,0)
		stop
}}

ACTOR STATICRecombine : Inventory {Inventory.MaxAmount 1}
