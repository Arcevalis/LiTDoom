Actor RocketSpawn //Replaces RocketAmmo
{
    States
    {

	 Spawn:
		SOSP A 0
		MARN A 0 ACS_NamedExecuteAlways("BDDisableNewGuns", 0, 0, 0, 0)//Check if advanced decorations are disabled.
		TNT1 A 2
		TNT1 A 0 A_Jump(30, "ROCKET", "Mine","Cluster")
		TNT1 A 0 A_JUMP(1, "warhead")
		TNT1 A 0 A_JUMP(5, "VIAL")
		
		GRENADE:
		TNT1 A 0 A_SpawnItemEx ("GrenadeAmmo",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		Stop
	
		warhead:
		TNT1 A 0 A_SpawnItemEx ("nuke",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		Stop
	
		ROCKET:
		TNT1 A 0 A_SpawnItemEx ("AmmoRocket",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,50)
		Stop
		
		Mine:
		TNT1 A 0 A_SpawnItemEx ("proxbomb",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		Stop	
		
		Cluster:
		TNT1 A 0 A_SpawnItemEx ("Clusterammo",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)	
		Stop
		
		Vial:
		TNT1 A 0 A_JUMP(126, 2)
		TNT1 A 0 A_SpawnItemEx ("HellfireAmmo",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		
		Stop
		TNT1 A 0 A_SpawnItemEx ("Acidmistammo",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)	
		Stop
	}
}		
	
