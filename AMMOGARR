

///SPAWNERS


ACTOR ShellSpawner : CustomInventory 
{
    States
    {

	 Spawn:
		TNT1 A 0
		MARN A 0 ACS_NamedExecuteAlways("BDDisableNewGuns", 0, 0, 0, 0)//Check if advanced decorations are disabled.
		TNT1 A 2
		TNT1 A 0 A_CheckProximity("RevolverAmmo","RevolverSPAWNER", 200, 1)
		TNT1 A 0 A_CheckProximity("Rifleammo","RifleSpawner", 200, 1)
		TNT1 A 0 A_Jump(35, "Spawn3")
		TNT1 A 0 A_Jump(140, "Spawn2")
		
	Vanilla:	
	Spawn1:
		TNT1 A 0
		TNT1 A 0 A_SpawnItemEx ("AmmoShell",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		TNT1 A 1
		Stop
	
	 Spawn2:
		TNT1 A 0
		TNT1 A 0 A_SpawnItemEx ("AmmoShellSmall",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		TNT1 A 1
		Stop

	 Spawn3:
		TNT1 A 0
		TNT1 A 0 A_SpawnItemEx ("AmmoSlug",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		TNT1 A 1
		Stop
		
		RevolverAmmo:
		TNT1 A 0
		TNT1 A 0 A_SpawnItemEx ("50ammo",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		TNT1 A 1
		Stop
		
		Rifleammo:
		TNT1 A 0 A_Jump(127, 2)
		TNT1 A 0 A_SpawnItemEx ("Clip2",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		Stop
		TNT1 A 0 A_SpawnItemEx ("Clip3",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		stop
	}
}

ACTOR ShellFuel //Replaces SHellBox
{
    States
    {

	 Spawn:
		TNT1 A 0
		MARN A 0 ACS_NamedExecuteAlways("BDDisableNewGuns", 0, 0, 0, 0)//Check if advanced decorations are disabled.
		TNT1 A 2
		TNT1 A 0 A_CheckProximity("Rifleammo","RifleSpawner", 200, 1)
		TNT1 A 0 A_Jump(15, "Spawn4")
		TNT1 A 0 A_Jump(35, "Spawn3")
		TNT1 A 0 A_Jump(100, "Spawn2")
		
	Vanilla:	
	Spawn1:
		TNT1 A 0
		TNT1 A 0 A_SpawnItemEx ("AmmoShellBox",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		TNT1 A 1
		Stop
	
	 Spawn2:
		TNT1 A 0
		TNT1 A 0 A_SpawnItemEx ("GASAMMO",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		TNT1 A 1
		Stop
		
	 Spawn3:
		TNT1 A 0
		TNT1 A 0 A_SpawnItemEx ("SlugBox",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		TNT1 A 1
		Stop
		
	 Spawn4:
		TNT1 A 0
		TNT1 A 0 A_SpawnItemEx ("Closedcrate",0,0,0,0,0,0,0,SXF_TRANSFERAMBUSHFLAG ,0)
		TNT1 A 1
		Stop		
		
	Rifleammo:
		TNT1 A 0
		TNT1 A 0 A_SpawnItemEx ("clipbox2",0,0,0,0,0,0,0,SXF_TRANSFERAMBUSHFLAG ,0)
		TNT1 A 1
		Stop	
	}
}


ACTOR Rocknade //Replaces RocketBox
{
    States
    {

	 Spawn:
		SOSP A 0
		MARN A 0 ACS_NamedExecuteAlways("BDDisableNewGuns", 0, 0, 0, 0)//Check if advanced decorations are disabled.
		TNT1 A 2
		TNT1 A 0 A_Jump(15, "Crate")
		TNT1 A 0 A_Jump(3, "warhead")
		TNT1 A 0 A_Jump(50, "EXPBOX","PROXBOMB","ROCKETS")
		
	GRENADEBOX:
		TNT1 A 0 A_SpawnItemEx ("GrenadeBox",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		Stop
	
	 EXPBOX:
		TNT1 A 0 A_SpawnItemEx ("ExpBox",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		Stop
	
	ROCKETS:
		TNT1 A 0 A_SpawnItemEx ("AMMOROCKETBOX",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		Stop
		
	PROXBOMB:
		TNT1 AA 0 A_SpawnItemEx ("proxbombammo",0,0,0,RANDOM(-2,2),RANDOM(-2,2),0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		Stop

	warhead:
		TNT1 A 0 A_SpawnItemEx ("nuke",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG ,0)
		Stop
		
	 Crate:
		TNT1 A 0 A_SpawnItemEx ("Closedcrate",0,0,0,0,0,0,0,SXF_TRANSFERAMBUSHFLAG ,0)
		Stop
	}
}

