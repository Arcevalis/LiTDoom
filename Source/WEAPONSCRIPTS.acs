Script "PLASMATEMPERATURE" (void)
{	
DELAY(1);
        if (Checkinventory("PLASMATEMPERATURE") >= 400)
        { DELAY(5);
		PLAYSOUND(0,"PLSBOOP",0);
		RESTART; }
		
		ELSE if (Checkinventory("PLASMATEMPERATURE") >= 250)
        { DELAY(15);
		PLAYSOUND(0,"PLSBOOP",0);
		RESTART; }
		
		ELSE if (Checkinventory("PLASMATEMPERATURE") >= 150)
        { DELAY(30);
		PLAYSOUND(0,"PLSBOOP",0);
		RESTART; }
		
		ELSE IF (Checkinventory("PLASMATEMPERATURE") < 150)
        { DELAY(10);
		RESTART; }
}

