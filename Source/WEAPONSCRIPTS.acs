Script "REALISTICRELOAD" ENTER
{
while (TRUE)
  {
	IF(GetCvar("realreload") == 0)
	{
	giveinventory("realreloadOFF",1);
	delay(1);
	}
	
	Else
	{
	takeinventory("realreloadOFF",1);
	delay(1);
	}
	delay(1);
  }
  }
 
 
 //// MOUNTED MACHINEGUN SPEED CHANGE
  
 script "HMGSPEED1" (void)
{ 
IF (checkinventory("UsedStamina") < 70 && GetPlayerInfo(PlayerNumber(), PLAYERINFO_PLAYERCLASS) == 0 || GetPlayerInfo(PlayerNumber(), PLAYERINFO_PLAYERCLASS) == 1)
{
ACS_NamedTerminate("Fatigue",0);
setactorproperty(0,APROP_Speed,0.1);
delay(1);
}

Else IF (checkinventory("UsedStamina") < 1400 && GetPlayerInfo(PlayerNumber(), PLAYERINFO_PLAYERCLASS) == 2)
{
ACS_Namedterminate("Fatigue",0);
setactorproperty(0,APROP_Speed,0.1);
delay(1);
}

Else
{
ACS_NamedTerminate("Fatigue",0);
SetPlayerProperty(false, ON, PROP_TOTALLYFROZEN);
delay(1);
}
}


script "HMGSPEED0" (void)
{SetPlayerProperty(false, ON, PROP_TOTALLYFROZEN);
DELAY(3);;
SetPlayerProperty(false, OFF, PROP_TOTALLYFROZEN);
ACS_Namedexecute("Fatigue",0);
}


/// QUICK KNIFE

Script "QUICKKNIFE" (void)
{
 IF (Checkinventory("KnivesAmmo") >= 1)
 {
 giveinventory("slashtoken",1);
 delay(1);
 terminate;
 }
 
 Else
 {
 Print(s:"No knives left");
  delay(1);
 terminate;
 }
}

