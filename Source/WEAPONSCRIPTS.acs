 Script "DURABILITYTOGGLE" ENTER
 {
	IF(GetCvar("Firearmsdurability") == 1)
	{
	takeinventory("firearmsdurabilityOFF",1);
	giveinventory("firearmsdurabilityON",1);
	terminate;
	}
	
	Else
	{
	giveinventory("firearmsdurabilityOFF",1);
	takeinventory("firearmsdurabilityON",1);
	terminate;
	}
}


