}

Script "PICKFLARE" (void)
{
	SetActivator(0, AAPTR_PLAYER1);
	giveinventory("LitflarePickup",1);

	IF (Checkinventory("pistolselected") >= 1)
		{
		delay(1);
		giveinventory("startdualwield",1);
		setweapon("NewFlare");
		}

	Else	
		{
		setweapon("NewFlare");
		}
	
	delay(40);
	takeinventory("LitflarePickup",1);
	terminate;
}



